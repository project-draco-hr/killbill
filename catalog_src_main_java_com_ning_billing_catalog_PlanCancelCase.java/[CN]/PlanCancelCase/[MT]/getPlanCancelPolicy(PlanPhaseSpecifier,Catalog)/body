{
  if ((phaseType == null || planPhase.getPhaseType() == phaseType) && (product == null || product.equals(c.getProductFromName(planPhase.getProductName()))) && (billingPeriod == null || billingPeriod.equals(planPhase.getBillingPeriod()))&& (priceList == null || priceList.equals(c.getPriceListFromName(planPhase.getPriceListName())))) {
    return getPolicy();
  }
  return null;
}
