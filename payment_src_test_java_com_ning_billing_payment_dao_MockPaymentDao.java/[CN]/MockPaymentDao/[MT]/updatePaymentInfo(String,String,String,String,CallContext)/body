{
  PaymentInfo existingPayment=payments.get(paymentId);
  if (existingPayment != null) {
    PaymentInfo payment=existingPayment.cloner().setPaymentMethod(paymentMethodType).setCardType(cardType).setCardCountry(cardCountry).setUpdatedDate(context.getUpdatedDate()).build();
    payments.put(paymentId,payment);
  }
}
