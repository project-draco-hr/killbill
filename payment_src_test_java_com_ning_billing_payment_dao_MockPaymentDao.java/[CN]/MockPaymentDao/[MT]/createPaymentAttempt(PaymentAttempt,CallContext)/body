{
  PaymentAttempt updatedPaymentAttempt=new DefaultPaymentAttempt(paymentAttempt.getId(),paymentAttempt.getInvoiceId(),paymentAttempt.getAccountId(),paymentAttempt.getAmount(),paymentAttempt.getCurrency(),paymentAttempt.getInvoiceDate(),paymentAttempt.getPaymentAttemptDate(),paymentAttempt.getPaymentId(),paymentAttempt.getRetryCount());
  paymentAttempts.put(updatedPaymentAttempt.getId(),updatedPaymentAttempt);
  return updatedPaymentAttempt;
}
