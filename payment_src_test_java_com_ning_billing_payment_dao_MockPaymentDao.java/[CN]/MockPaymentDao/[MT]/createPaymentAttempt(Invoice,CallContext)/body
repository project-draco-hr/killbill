{
  PaymentAttempt updatedPaymentAttempt=new PaymentAttempt(UUID.randomUUID(),invoice.getId(),invoice.getAccountId(),invoice.getBalance(),invoice.getCurrency(),invoice.getInvoiceDate(),null,null,null,context.getCreatedDate(),context.getUpdatedDate());
  paymentAttempts.put(updatedPaymentAttempt.getPaymentAttemptId(),updatedPaymentAttempt);
  return updatedPaymentAttempt;
}
