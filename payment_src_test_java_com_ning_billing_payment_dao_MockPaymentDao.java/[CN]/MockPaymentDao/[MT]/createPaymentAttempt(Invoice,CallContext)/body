{
  PaymentAttempt updatedPaymentAttempt=new DefaultPaymentAttempt(UUID.randomUUID(),invoice.getId(),invoice.getAccountId(),invoice.getBalance(),invoice.getCurrency(),invoice.getInvoiceDate(),null,null,null,null,null);
  paymentAttempts.put(updatedPaymentAttempt.getId(),updatedPaymentAttempt);
  return updatedPaymentAttempt;
}
