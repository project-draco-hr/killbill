{
  PaymentAttempt updatedPaymentAttempt=new PaymentAttempt(paymentAttempt.getPaymentAttemptId(),paymentAttempt.getInvoiceId(),paymentAttempt.getAccountId(),paymentAttempt.getAmount(),paymentAttempt.getCurrency(),paymentAttempt.getInvoiceDate(),paymentAttempt.getPaymentAttemptDate(),paymentAttempt.getPaymentId(),paymentAttempt.getRetryCount(),paymentAttemptStatus,context.getCreatedDate(),context.getUpdatedDate());
  paymentAttempts.put(updatedPaymentAttempt.getPaymentAttemptId(),updatedPaymentAttempt);
  return updatedPaymentAttempt;
}
