{
  PaymentAttempt existingPaymentAttempt=paymentAttempts.get(paymentAttemptId);
  if (existingPaymentAttempt != null) {
    paymentAttempts.put(existingPaymentAttempt.getPaymentAttemptId(),new PaymentAttempt(existingPaymentAttempt.getPaymentAttemptId(),existingPaymentAttempt.getInvoice(),paymentId));
  }
}
