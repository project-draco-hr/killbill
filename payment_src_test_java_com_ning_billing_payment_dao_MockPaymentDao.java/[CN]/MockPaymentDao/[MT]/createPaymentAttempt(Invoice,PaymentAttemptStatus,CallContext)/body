{
  PaymentAttempt updatedPaymentAttempt=new DefaultPaymentAttempt(UUID.randomUUID(),invoice.getId(),invoice.getAccountId(),invoice.getBalance(),invoice.getCurrency(),invoice.getInvoiceDate(),null,null,null,context.getCreatedDate(),context.getUpdatedDate(),paymentAttemptStatus);
  paymentAttempts.put(updatedPaymentAttempt.getId(),updatedPaymentAttempt);
  return updatedPaymentAttempt;
}
