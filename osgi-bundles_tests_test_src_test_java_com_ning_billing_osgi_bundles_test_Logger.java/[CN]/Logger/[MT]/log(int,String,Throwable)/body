{
  final Object log=logTracker.getService();
  if (log != null) {
    if (t == null) {
      ((LogService)log).log(level,message);
    }
 else {
      ((LogService)log).log(level,message,t);
    }
  }
 else {
    if (level >= 2) {
      System.out.println(message);
    }
 else {
      System.err.println(message);
    }
    if (t != null) {
      t.printStackTrace(System.err);
    }
  }
}
