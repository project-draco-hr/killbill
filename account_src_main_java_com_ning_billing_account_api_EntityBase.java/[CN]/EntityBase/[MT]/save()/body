{
  IEventBusType event;
  if (isNew) {
    event=getCreateEvent();
    saveObject();
  }
 else {
    event=getUpdateEvent();
    updateObject();
  }
  if (event != null) {
    try {
      eventBus.post(event);
    }
 catch (    IEventBus.EventBusException evbe) {
      log.error("Failed to post account change to event bus during save.",evbe);
    }
  }
}
