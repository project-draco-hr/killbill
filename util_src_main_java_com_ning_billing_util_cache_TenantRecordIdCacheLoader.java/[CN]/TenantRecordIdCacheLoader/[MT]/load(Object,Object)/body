{
  checkCacheLoaderStatus();
  if (!(argument instanceof ObjectType)) {
    throw new IllegalArgumentException("Unexpected argument type of " + argument != null ? argument.getClass().getName() : "null");
  }
  if (!(key instanceof String)) {
    throw new IllegalArgumentException("Unexpected key type of " + key != null ? key.getClass().getName() : "null");
  }
  final String objectId=(String)key;
  final ObjectType objectType=(ObjectType)argument;
  Long value=nonEntityDao.retrieveTenantRecordIdFromObject(UUID.fromString(objectId),objectType,null);
  return value;
}
