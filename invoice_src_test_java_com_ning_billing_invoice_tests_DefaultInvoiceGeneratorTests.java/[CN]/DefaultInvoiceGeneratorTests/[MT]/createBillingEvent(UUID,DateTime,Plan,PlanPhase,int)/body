{
  Subscription sub=new SubscriptionData(new SubscriptionBuilder().setId(subscriptionId));
  Currency currency=Currency.USD;
  return new DefaultBillingEvent(sub,startDate,plan,planPhase,planPhase.getFixedPrice() == null ? null : planPhase.getFixedPrice().getPrice(currency),planPhase.getRecurringPrice() == null ? null : planPhase.getRecurringPrice().getPrice(currency),currency,planPhase.getBillingPeriod(),billCycleDay,BillingModeType.IN_ADVANCE,"Test",SubscriptionTransitionType.CREATE);
}
