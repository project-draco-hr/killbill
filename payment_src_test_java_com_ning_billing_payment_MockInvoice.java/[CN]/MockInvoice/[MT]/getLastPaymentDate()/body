{
  DateTime lastPayment=null;
  for (  final InvoicePayment payment : payments) {
    final DateTime paymentDate=payment.getPaymentDate();
    if (lastPayment == null) {
      lastPayment=paymentDate;
    }
    if (lastPayment.isBefore(paymentDate)) {
      lastPayment=paymentDate;
    }
  }
  return lastPayment;
}
