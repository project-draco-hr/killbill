{
  final PlanPhase phase=Mockito.mock(PlanPhase.class);
  Mockito.when(phase.getName()).thenReturn("phase");
  final Plan plan=Mockito.mock(Plan.class);
  Mockito.when(plan.getName()).thenReturn("plan");
