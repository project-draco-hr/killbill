{
  for (  SubscriptionTimeline cur : input) {
    boolean found=false;
    for (    Subscription s : subscriptions) {
      if (s.getId().equals(cur.getId())) {
        found=true;
        break;
      }
    }
    if (!found) {
      throw new EntitlementRepairException(ErrorCode.ENT_REPAIR_UNKNOWN_SUBSCRIPTION,cur.getId());
    }
  }
}
