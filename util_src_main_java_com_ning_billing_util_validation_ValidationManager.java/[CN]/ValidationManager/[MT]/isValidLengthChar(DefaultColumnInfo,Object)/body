{
  if (columnInfo.getDataType().equals("char") || columnInfo.getDataType().equals(String.valueOf(Types.CHAR))) {
    if (value == null) {
      return false;
    }
 else {
      if (value.toString().length() != columnInfo.getMaximumLength()) {
        return false;
      }
    }
  }
  return true;
}
