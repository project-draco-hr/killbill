{
  final Pattern pattern=Pattern.compile("number=([^;]+)");
  final ImmutableList<Pattern> patterns=ImmutableList.<Pattern>of(pattern);
  Assert.assertEquals(obfuscator.obfuscate("number=1234;number=12345;number=123456;number=1234567;number=12345678;number=123456789",patterns),"number=MASKED;number=MASKED;number=MASKED;number=MASKED*;number=*MASKED*;number=*MASKED**");
}
