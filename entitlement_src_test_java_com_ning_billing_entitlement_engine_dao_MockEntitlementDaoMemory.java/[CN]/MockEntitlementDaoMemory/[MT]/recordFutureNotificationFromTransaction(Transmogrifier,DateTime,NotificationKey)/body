{
  try {
    NotificationQueue subscritionEventQueue=notificationQueueService.getNotificationQueue(Engine.ENTITLEMENT_SERVICE_NAME,Engine.NOTIFICATION_QUEUE_NAME);
    subscritionEventQueue.recordFutureNotificationFromTransaction(transactionalDao,effectiveDate,notificationKey);
  }
 catch (  NoSuchNotificationQueue e) {
    throw new RuntimeException(e);
  }
}
