{
  final Clock clock=new DefaultClock();
  final InvoiceConfig invoiceConfig=new InvoiceConfig(){
    @Override public int getNumberOfMonthsInFuture(){
      return 36;
    }
    @Override public boolean isEmailNotificationsEnabled(){
      return false;
    }
    @Override public boolean isInsertZeroUsageItems(){
      return true;
    }
  }
;
  this.generator=new DefaultInvoiceGenerator(clock,null,invoiceConfig,null,controllerDispatcher);
}
