{
  PaymentInfo payment=new PaymentInfo.Builder().setPaymentId(UUID.randomUUID().toString()).setAmount(invoice.getBalance()).setStatus("Processed").setBankIdentificationNumber("1234").setCreatedDate(new DateTime()).setEffectiveDate(new DateTime()).setPaymentNumber("12345").setReferenceId("12345").setType("Electronic").build();
  return Either.right(payment);
}
