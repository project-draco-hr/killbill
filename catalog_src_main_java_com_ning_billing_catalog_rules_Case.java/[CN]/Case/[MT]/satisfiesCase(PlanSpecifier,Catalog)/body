{
  return (product == null || product.equals(c.getProductFromName(planPhase.getProductName()))) && (productCategory == null || productCategory.equals(planPhase.getProductCategory())) && (billingPeriod == null || billingPeriod.equals(planPhase.getBillingPeriod()))&& (priceList == null || priceList.equals(c.getPriceListFromName(planPhase.getPriceListName())));
}
