{
  testListener.pushExpectedEvent(NextEvent.CREATE);
  final DefaultSubscriptionBase subscription=(DefaultSubscriptionBase)subscriptionApi.createSubscription(bundleId,new PlanPhaseSpecifier(productName,ProductCategory.BASE,term,planSet,null),requestedDate == null ? clock.getUTCNow() : requestedDate,internalCallContext);
  assertNotNull(subscription);
  testListener.assertListenerStatus();
  return subscription;
}
