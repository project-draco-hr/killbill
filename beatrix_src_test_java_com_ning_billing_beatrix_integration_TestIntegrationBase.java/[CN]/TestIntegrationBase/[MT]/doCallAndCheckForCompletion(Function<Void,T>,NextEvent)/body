{
  Joiner joiner=Joiner.on(", ");
  log.info("            ************    STARTING BUS HANDLER CHECK : {} ********************",joiner.join(events));
  busHandler.pushExpectedEvents(events);
  final T result=f.apply(null);
  assertListenerStatus();
  log.info("            ************    DONE WITH BUS HANDLER CHECK    ********************");
  return result;
}
