{
  return doCallAndCheckForCompletion(new Function<Void,Subscription>(){
    @Override public Subscription apply(    @Nullable final Void dontcare){
      try {
        final Subscription subscription=entitlementUserApi.createSubscription(bundleId,new PlanPhaseSpecifier(productName,productCategory,billingPeriod,PriceListSet.DEFAULT_PRICELIST_NAME,null),null,callContext);
        assertNotNull(subscription);
        return subscription;
      }
 catch (      SubscriptionUserApiException e) {
        fail();
        return null;
      }
    }
  }
,events);
}
