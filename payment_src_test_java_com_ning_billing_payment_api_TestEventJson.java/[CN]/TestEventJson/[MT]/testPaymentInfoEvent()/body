{
  PaymentInfoEvent e=new DefaultPaymentInfoEvent(UUID.randomUUID(),UUID.randomUUID(),UUID.randomUUID().toString(),new BigDecimal(12),new BigDecimal(12.9),"BNP","eeert","success","credit","ref","paypal","paypal","","",UUID.randomUUID(),new DateTime(),new DateTime(),new DateTime());
  String json=mapper.writeValueAsString(e);
  Class<?> claz=Class.forName(DefaultPaymentInfoEvent.class.getName());
  Object obj=mapper.readValue(json,claz);
  Assert.assertTrue(obj.equals(e));
}
