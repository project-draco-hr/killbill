{
  if (zeroPrice == null) {
    Currency[] currencies=root.getSupportedCurrencies();
    zeroPrice=new Price[currencies.length];
    for (int i=0; i < currencies.length; i++) {
      prices[i]=new Price();
      prices[i].setCurrency(currencies[i]);
      prices[i].setValue(new BigDecimal(0));
    }
  }
  return zeroPrice;
}
