{
  return paymentProcessor.createPurchase(paymentStateControlContext.isApiPayment(),paymentStateControlContext.getAttemptId(),paymentStateControlContext.getAccount(),paymentStateControlContext.getPaymentMethodId(),paymentStateControlContext.getPaymentId(),paymentStateControlContext.getAmount(),paymentStateControlContext.getCurrency(),paymentStateControlContext.getPaymentExternalKey(),paymentStateControlContext.getPaymentTransactionExternalKey(),false,paymentStateControlContext.getProperties(),paymentStateControlContext.getCallContext(),paymentStateControlContext.getInternalCallContext());
}
