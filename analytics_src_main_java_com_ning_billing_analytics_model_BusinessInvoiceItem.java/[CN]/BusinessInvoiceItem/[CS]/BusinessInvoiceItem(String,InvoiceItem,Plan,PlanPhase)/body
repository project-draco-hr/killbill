{
  this(invoiceItem.getAmount(),planPhase != null ? planPhase.getBillingPeriod().toString() : null,new DateTime(DateTimeZone.UTC),invoiceItem.getCurrency(),invoiceItem.getEndDate(),externalKey,invoiceItem.getInvoiceId(),invoiceItem.getId(),invoiceItem.getInvoiceItemType().toString(),planPhase != null ? planPhase.getPhaseType().toString() : null,plan != null ? plan.getProduct().getCategory().toString() : null,plan != null ? plan.getProduct().getName() : null,plan != null ? plan.getProduct().getCatalogName() : null,planPhase != null ? planPhase.getName() : null,invoiceItem.getStartDate(),new DateTime(DateTimeZone.UTC));
}
