{
  this(invoiceItem.getAmount(),plan.getBillingPeriod().toString(),new DateTime(DateTimeZone.UTC),invoiceItem.getCurrency(),invoiceItem.getEndDate(),externalKey,invoiceItem.getInvoiceId(),invoiceItem.getId(),invoiceItem.getInvoiceItemType().toString(),invoiceItem.getPhaseName(),plan.getProduct().getName(),plan.getProduct().getCatalogName(),plan.getProduct().getCategory().toString(),planPhase.getName(),invoiceItem.getStartDate(),new DateTime(DateTimeZone.UTC));
}
