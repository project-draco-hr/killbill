{
  this(invoiceItem.getAmount(),planPhase.getBillingPeriod().toString(),new DateTime(DateTimeZone.UTC),invoiceItem.getCurrency(),invoiceItem.getEndDate(),externalKey,invoiceItem.getInvoiceId(),invoiceItem.getId(),invoiceItem.getInvoiceItemType().toString(),planPhase.getPhaseType().toString(),plan.getProduct().getCategory().toString(),plan.getProduct().getName(),plan.getProduct().getCatalogName(),planPhase.getName(),invoiceItem.getStartDate(),new DateTime(DateTimeZone.UTC));
}
