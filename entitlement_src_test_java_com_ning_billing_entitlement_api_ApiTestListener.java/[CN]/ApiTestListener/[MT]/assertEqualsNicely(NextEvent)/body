{
  boolean foundIt=false;
  Iterator<NextEvent> it=nextExpectedEvent.iterator();
  while (it.hasNext()) {
    NextEvent ev=it.next();
    if (ev == expected) {
      it.remove();
      foundIt=true;
      break;
    }
  }
  if (!foundIt) {
    Joiner joiner=Joiner.on(" ");
    Assert.fail("Expected event " + expected + " got "+ joiner.join(nextExpectedEvent));
  }
}
