{
  if (expected != real) {
    System.err.println("Expected event " + expected + " got "+ real);
    try {
      NextEvent next=nextExpectedEvent.pop();
      while (next != null) {
        System.err.println("Also got event " + next);
        next=nextExpectedEvent.pop();
      }
    }
 catch (    EmptyStackException ignore) {
    }
    System.exit(1);
  }
}
