{
  testListener.pushExpectedEvent(NextEvent.CREATE);
  SubscriptionData subscription=(SubscriptionData)entitlementApi.createSubscription(bundleId,new PlanPhaseSpecifier(productName,ProductCategory.BASE,term,planSet,null),clock.getUTCNow(),context);
  assertNotNull(subscription);
  assertTrue(testListener.isCompleted(5000));
  return subscription;
}
