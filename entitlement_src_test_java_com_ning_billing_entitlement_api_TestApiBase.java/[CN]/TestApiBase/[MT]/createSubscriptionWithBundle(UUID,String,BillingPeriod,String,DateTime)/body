{
  testListener.pushNextApiExpectedEvent(NextEvent.CREATE);
  SubscriptionData subscription=(SubscriptionData)entitlementApi.createSubscription(bundleId,new PlanPhaseSpecifier(productName,ProductCategory.BASE,term,planSet,null),requestedDate == null ? clock.getUTCNow() : requestedDate,context);
  assertNotNull(subscription);
  assertTrue(testListener.isApiCompleted(5000));
  return subscription;
}
