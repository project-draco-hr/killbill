{
  testListener.pushExpectedEvent(NextEvent.CREATE);
  final SubscriptionData subscription=(SubscriptionData)entitlementApi.createSubscription(bundleId,new PlanPhaseSpecifier(productName,ProductCategory.BASE,term,planSet,null),requestedDate == null ? clock.getUTCNow() : requestedDate,context);
  assertNotNull(subscription);
  assertTrue(testListener.isCompleted(5000));
  return subscription;
}
