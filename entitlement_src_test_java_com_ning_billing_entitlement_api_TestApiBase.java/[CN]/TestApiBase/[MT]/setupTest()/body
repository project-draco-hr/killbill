{
  log.warn("RESET TEST FRAMEWORK\n\n");
  if (testListener != null) {
    testListener.reset();
  }
  clock.resetDeltaFromReality();
  ((MockEntitlementDao)dao).reset();
  busService.getBus().register(testListener);
