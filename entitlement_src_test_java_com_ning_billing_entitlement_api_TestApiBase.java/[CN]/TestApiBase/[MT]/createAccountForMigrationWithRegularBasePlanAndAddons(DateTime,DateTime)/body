{
  final List<EntitlementSubscriptionMigrationCaseWithCTD> cases=new LinkedList<EntitlementSubscriptionMigrationCaseWithCTD>();
  cases.add(new EntitlementSubscriptionMigrationCaseWithCTD(new PlanPhaseSpecifier("Shotgun",ProductCategory.BASE,BillingPeriod.ANNUAL,PriceListSet.DEFAULT_PRICELIST_NAME,PhaseType.EVERGREEN),initialBPstart,null,initialBPstart.plusYears(1)));
