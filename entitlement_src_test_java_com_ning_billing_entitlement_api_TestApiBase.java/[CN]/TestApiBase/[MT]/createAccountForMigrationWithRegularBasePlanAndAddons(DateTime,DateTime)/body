  firstAddOnCases.add(new EntitlementSubscriptionMigrationCaseWithCTD(new PlanPhaseSpecifier("Telescopic-Scope",ProductCategory.ADD_ON,BillingPeriod.MONTHLY,PriceListSet.DEFAULT_PRICELIST_NAME,PhaseType.EVERGREEN),initalAddonStart.plusMonths(1),null,null));
