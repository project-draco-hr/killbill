{
  Matcher m=KillbillActivator.PLUGIN_NAME_PATTERN.matcher("1abd");
  Assert.assertFalse(m.matches());
  m=KillbillActivator.PLUGIN_NAME_PATTERN.matcher("Tata");
  Assert.assertFalse(m.matches());
  m=KillbillActivator.PLUGIN_NAME_PATTERN.matcher("Tata#");
  Assert.assertFalse(m.matches());
  m=KillbillActivator.PLUGIN_NAME_PATTERN.matcher("yo:");
  Assert.assertFalse(m.matches());
}
