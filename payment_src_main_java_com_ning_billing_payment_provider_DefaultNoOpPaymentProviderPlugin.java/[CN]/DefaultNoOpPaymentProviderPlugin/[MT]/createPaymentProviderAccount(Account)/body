{
  if (account != null) {
    final String id=UUID.randomUUID().toString();
    final String paymentMethodId=UUID.randomUUID().toString();
    accounts.put(account.getExternalKey(),new PaymentProviderAccount.Builder().setAccountKey(account.getExternalKey()).setId(id).setDefaultPaymentMethod(paymentMethodId).build());
    return id;
  }
 else {
    throw new PaymentPluginApiException("","Did not get account to create payment provider account");
  }
}
