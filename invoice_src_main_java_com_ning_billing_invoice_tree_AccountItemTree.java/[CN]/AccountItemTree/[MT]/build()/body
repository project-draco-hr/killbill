{
  Preconditions.checkState(!isBuilt);
  if (allExistingItems.size() > 0) {
    for (    InvoiceItem item : allExistingItems) {
      addExistingItem(item,true);
    }
    allExistingItems.clear();
  }
  for (  SubscriptionItemTree tree : subscriptionItemTree.values()) {
    tree.build();
  }
  isBuilt=true;
}
