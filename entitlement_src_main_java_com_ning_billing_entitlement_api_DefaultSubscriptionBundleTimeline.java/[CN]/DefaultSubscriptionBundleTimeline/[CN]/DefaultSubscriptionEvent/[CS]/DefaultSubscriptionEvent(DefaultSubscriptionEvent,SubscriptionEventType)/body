{
  this(copy.getId(),copy.getEntitlementId(),copy.getEffectiveDate(),copy.getRequestedDate(),newEventType,copy.isBlockedEntitlement(),copy.isBlockedBilling(),copy.getServiceName(),copy.getServiceStateName(),copy.getPrevProduct(),copy.getPrevPlan(),copy.getPrevPhase(),copy.getPrevPriceList(),copy.getPrevBillingPeriod(),copy.getNextProduct(),copy.getNextPlan(),copy.getNextPhase(),copy.getNextPriceList(),copy.getNextBillingPeriod(),copy.getCreatedDate());
}
