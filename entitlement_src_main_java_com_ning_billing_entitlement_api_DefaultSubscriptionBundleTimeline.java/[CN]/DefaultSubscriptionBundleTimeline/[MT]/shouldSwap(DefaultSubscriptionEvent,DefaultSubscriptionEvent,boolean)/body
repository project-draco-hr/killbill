{
  final int idComp=cur.getEntitlementId().compareTo(other.getEntitlementId());
  return (cur.getEffectiveDate().compareTo(other.getEffectiveDate()) == 0 && ((isAscending && ((idComp > 0) || (idComp == 0 && cur.getSubscriptionEventType().ordinal() > other.getSubscriptionEventType().ordinal()))) || (!isAscending && ((idComp < 0) || (idComp == 0 && cur.getSubscriptionEventType().ordinal() < other.getSubscriptionEventType().ordinal())))));
}
