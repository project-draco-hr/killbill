{
  return new DefaultSubscriptionEvent(in.getId(),entitlementId,new LocalDate(in.getEffectiveDate(),accountTimeZone),new LocalDate(in.getCreatedDate(),accountTimeZone),eventType,in.isBlockEntitlement(),in.isBlockBilling(),in.getService(),in.getStateName(),prev != null ? prev.getNextProduct() : null,prev != null ? prev.getNextPlan() : null,prev != null ? prev.getNextPhase() : null,prev != null ? prev.getNextPriceList() : null,prev != null ? prev.getNextBillingPeriod() : null,next != null ? next.getPrevProduct() : null,next != null ? next.getPrevPlan() : null,next != null ? next.getPrevPhase() : null,next != null ? next.getPrevPriceList() : null,next != null ? next.getPrevBillingPeriod() : null,in.getCreatedDate());
}
