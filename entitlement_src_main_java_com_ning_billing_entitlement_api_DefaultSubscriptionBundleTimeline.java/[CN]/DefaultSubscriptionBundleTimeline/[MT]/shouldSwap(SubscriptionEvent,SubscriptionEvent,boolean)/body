{
  final int idComp=cur.getEntitlementId().compareTo(other.getEntitlementId());
  return (cur.getEffectiveDate().compareTo(other.getEffectiveDate()) == 0 && ((isAscending && ((idComp > 0) || (idComp == 0 && compareSubscriptionEventsForSameEffectiveDateAndEntitlementId(cur,other) > 0))) || (!isAscending && ((idComp < 0) || (idComp == 0 && compareSubscriptionEventsForSameEffectiveDateAndEntitlementId(cur,other) < 0)))));
}
