{
  final List<String> tags=new ArrayList<String>();
  for (  final Tag tag : account.getTagList()) {
    tags.add(tag.getTagDefinitionName());
  }
  final BusinessAccount bac=new BusinessAccount(account.getExternalKey(),invoiceUserApi.getAccountBalance(account.getId()),tags,null,null,null,null,null,null);
  updateBusinessAccountFromAccount(account,bac);
  return bac;
}
