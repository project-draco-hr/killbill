{
  if (fixed != null) {
    fixed.initialize(root,uri);
    fixed.setPhase(this);
  }
  if (recurring != null) {
    recurring.initialize(root,uri);
    recurring.setPhase(this);
  }
  if (usages != null) {
    for (    DefaultUsage usage : usages) {
      usage.initialize(root,uri);
      usage.setPhase(this);
    }
  }
}
