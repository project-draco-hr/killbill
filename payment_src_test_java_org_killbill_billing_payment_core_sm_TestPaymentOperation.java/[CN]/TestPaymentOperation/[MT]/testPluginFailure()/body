{
  setUp(null);
  Assert.assertNull(paymentStateContext.getPaymentInfoPlugin());
  try {
    paymentOperation.doOperationCallback();
    Assert.fail();
  }
 catch (  final OperationException e) {
    Assert.assertEquals(e.getOperationResult(),OperationResult.EXCEPTION);
  }
  Assert.assertNull(paymentStateContext.getPaymentInfoPlugin());
}
