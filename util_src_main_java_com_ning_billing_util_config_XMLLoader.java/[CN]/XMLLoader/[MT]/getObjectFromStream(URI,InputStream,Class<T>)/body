{
  Object o=unmarshaller(clazz).unmarshal(stream);
  if (clazz.isInstance(o)) {
    @SuppressWarnings("unchecked") T castObject=(T)o;
    try {
      validate(uri,castObject);
    }
 catch (    ValidationException e) {
      e.getErrors().log(log);
      System.err.println(e.getErrors().toString());
      throw e;
    }
    return castObject;
  }
 else {
    return null;
  }
}
