{
  if (!isInitialized) {
    try {
      System.out.println("Overdue config URI" + properties.getConfigURI());
      URI u=new URI(properties.getConfigURI());
      if (u != null) {
        overdueConfig=XMLLoader.getObjectFromUri(u,OverdueConfig.class);
      }
      isInitialized=true;
    }
 catch (    Exception e) {
      throw new ServiceException(e);
    }
  }
}
