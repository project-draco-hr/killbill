{
  if (config.isProcessingOff() || !isStarted()) {
    return;
  }
  int nbQueueStarted=0;
synchronized (queues) {
    for (    final NotificationQueue cur : queues.values()) {
      if (cur.isStarted()) {
        nbQueueStarted++;
      }
    }
  }
  if (nbQueueStarted == 0) {
    super.stopQueue();
  }
}
