{
  PaymentInfoEvent paymentInfo=new DefaultPaymentInfo.Builder().setPaymentId(UUID.randomUUID().toString()).setAmount(BigDecimal.TEN).setStatus("Processed").setBankIdentificationNumber("1234").setPaymentNumber("12345").setPaymentMethodId("12345").setReferenceId("12345").setType("Electronic").setEffectiveDate(new DefaultClock().getUTCNow()).build();
  paymentDao.savePaymentInfo(paymentInfo,context);
}
