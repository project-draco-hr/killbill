{
  PaymentInfoEvent paymentInfo=new DefaultPaymentInfoEvent.Builder().setId(UUID.randomUUID()).setAmount(BigDecimal.TEN).setStatus("Processed").setBankIdentificationNumber("1234").setPaymentNumber("12345").setPaymentMethodId("12345").setReferenceId("12345").setType("Electronic").setEffectiveDate(new DefaultClock().getUTCNow()).build();
  paymentDao.insertPaymentInfoWithPaymentAttemptUpdate(paymentInfo,null,context);
}
