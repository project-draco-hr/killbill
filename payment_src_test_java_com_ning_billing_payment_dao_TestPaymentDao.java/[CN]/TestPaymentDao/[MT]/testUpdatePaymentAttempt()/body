{
  PaymentAttempt paymentAttempt=new DefaultPaymentAttempt.Builder().setPaymentAttemptId(UUID.randomUUID()).setPaymentId(UUID.randomUUID()).setInvoiceId(UUID.randomUUID()).setAccountId(UUID.randomUUID()).setAmount(BigDecimal.TEN).setCurrency(Currency.USD).setInvoiceDate(context.getCreatedDate()).build();
  paymentDao.createPaymentAttempt(paymentAttempt,PaymentAttemptStatus.IN_PROCESSING,context);
}
