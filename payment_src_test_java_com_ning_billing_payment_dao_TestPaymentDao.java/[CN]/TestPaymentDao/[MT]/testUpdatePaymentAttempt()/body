{
  PaymentAttempt paymentAttempt=new PaymentAttempt.Builder().setPaymentAttemptId(UUID.randomUUID()).setPaymentId(UUID.randomUUID().toString()).setInvoiceId(UUID.randomUUID()).setAccountId(UUID.randomUUID()).setAmount(BigDecimal.TEN).setCurrency(Currency.USD).setInvoiceDate(new DateTime(DateTimeZone.UTC)).setCreatedDate(new DateTime(DateTimeZone.UTC)).setUpdatedDate(new DateTime(DateTimeZone.UTC)).build();
  paymentDao.createPaymentAttempt(paymentAttempt);
}
