{
  dbi.withHandle(new HandleCallback<Void>(){
    @Override public Void withHandle(    final Handle handle) throws Exception {
      final String paymentId=Objects.firstNonNull(data.getPaymentId(),"").toString();
      final String paymentMethodId=Objects.firstNonNull(data.getPaymentMethodId(),"").toString();
      handle.execute("insert into _invoice_payment_control_plugin_auto_pay_off " + "(payment_external_key, transaction_external_key, account_id, plugin_name, payment_id, payment_method_id, amount, currency, created_by, created_date) values " + "(?,?,?,?,?,?,?,?,?,?)",data.getPaymentExternalKey(),data.getTransactionExternalKey(),data.getAccountId(),data.getPluginName(),paymentId,paymentMethodId,data.getAmount(),data.getCurrency(),data.getCreatedBy(),data.getCreatedDate());
      return null;
    }
  }
);
}
