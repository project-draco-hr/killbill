{
  String thisKey="test" + UUID.randomUUID().toString();
  String lastName=UUID.randomUUID().toString();
  String thisEmail="me@me.com" + " " + UUID.randomUUID();
  String firstName="Bob";
  String name=firstName + " " + lastName;
  String phone="123-456-7890";
  String locale="EN-US";
  DateTimeZone timeZone=DateTimeZone.forID("America/Los_Angeles");
  int firstNameLength=firstName.length();
  return new AccountBuilder().externalKey(thisKey).name(name).phone(phone).firstNameLength(firstNameLength).email(thisEmail).currency(Currency.USD).locale(locale).timeZone(timeZone);
}
