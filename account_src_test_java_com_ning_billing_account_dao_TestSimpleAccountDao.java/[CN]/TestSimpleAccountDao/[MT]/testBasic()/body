{
  IAccount a=new Account().withKey("foo");
  dao.createAccount(a);
  IAccount r=dao.getAccountByKey("foo");
  assertNotNull(r);
  assertEquals(r.getKey(),a.getKey());
  r=dao.getAccountById(r.getId());
  assertNotNull(r);
  assertEquals(r.getKey(),a.getKey());
  List<IAccount> all=dao.getAccounts();
  assertNotNull(all);
  assertTrue(all.size() >= 1);
}
