{
  String thisKey=key + UUID.randomUUID().toString();
  String lastName=UUID.randomUUID().toString();
  String thisEmail=email + " " + UUID.randomUUID();
  String name=firstName + " " + lastName;
  String phone="123-456-7890";
  DateTime createdDate=new DateTime(DateTimeZone.UTC);
  DateTime updatedDate=new DateTime(DateTimeZone.UTC);
  int firstNameLength=firstName.length();
  return new AccountBuilder().externalKey(thisKey).name(name).phone(phone).firstNameLength(firstNameLength).email(thisEmail).currency(Currency.USD).createdDate(createdDate).updatedDate(updatedDate).build();
}
