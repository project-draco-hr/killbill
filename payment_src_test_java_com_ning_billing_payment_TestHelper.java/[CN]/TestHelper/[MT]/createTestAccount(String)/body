  zombie.addResult("getFirstNameLength",10);
  zombie.addResult("getPhone","123-456-7890");
  zombie.addResult("getEmail",email);
  zombie.addResult("getCurrency",Currency.USD);
  zombie.addResult("getBillCycleDay",1);
