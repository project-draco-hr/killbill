{
  Invoice invoice=new DefaultInvoice(UUID.randomUUID(),account.getId(),new DateTime(),targetDate,currency);
  for (  InvoiceItem item : items) {
    invoice.addInvoiceItem(new DefaultInvoiceItem(invoice.getId(),item.getSubscriptionId(),item.getStartDate(),item.getEndDate(),item.getDescription(),item.getAmount(),item.getRate(),item.getCurrency()));
  }
  invoiceDao.create(invoice);
  return invoice;
}
