{
  return super.evaluate((BillingState)state,now) && (accountCondition == null || accountCondition.evaluate(state.getAccountState(),now)) && (basePlanProduct == null || basePlanProduct.equals(state.getBasePlanProduct()))&& (basePlanBillingPeriod == null || basePlanBillingPeriod.equals(state.getBasePlanBillingPeriod()))&& (basePlanPriceList == null || basePlanPriceList.equals(state.getBasePlanPriceList()));
}
