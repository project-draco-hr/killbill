{
  if (account != null) {
    PaymentProviderAccount paymentProviderAccount=accounts.put(account.getAccountName(),new PaymentProviderAccount.Builder().setAccountName(account.getAccountName()).setAccountNumber(account.getAccountName()).setId(account.getId()).build());
    return Either.right(paymentProviderAccount);
  }
 else {
    return Either.left(new PaymentError("unknown","Did not get account to create payment provider account"));
  }
}
