{
  if (account != null) {
    PaymentProviderAccount paymentProviderAccount=accounts.put(account.getExternalKey(),new PaymentProviderAccount.Builder().setAccountNumber(String.valueOf(RandomUtils.nextInt(10))).setDefaultPaymentMethod(String.valueOf(RandomUtils.nextInt(10))).setId(String.valueOf(RandomUtils.nextInt(10))).build());
    return Either.right(paymentProviderAccount.getId());
  }
 else {
    return Either.left(new PaymentError("unknown","Did not get account to create payment provider account"));
  }
}
