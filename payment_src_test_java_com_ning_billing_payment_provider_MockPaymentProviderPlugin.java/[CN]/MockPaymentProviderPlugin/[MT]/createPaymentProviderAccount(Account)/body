{
  if (account != null) {
    String id=String.valueOf(RandomStringUtils.random(10));
    accounts.put(account.getExternalKey(),new PaymentProviderAccount.Builder().setAccountNumber(String.valueOf(RandomStringUtils.random(10))).setDefaultPaymentMethod(String.valueOf(RandomStringUtils.random(10))).setId(id).build());
    return Either.right(id);
  }
 else {
    return Either.left(new PaymentError("unknown","Did not get account to create payment provider account"));
  }
}
