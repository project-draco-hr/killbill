{
  try {
    entitlementApi.createSubscription(bundleId,productName,term,planSet);
    assertFalse(true);
  }
 catch (  EntitlementUserApiException e) {
    assertEquals(e.getCode(),expected.getCode());
    try {
      log.info(e.getMessage());
    }
 catch (    Throwable el) {
      assertFalse(true);
    }
  }
}
