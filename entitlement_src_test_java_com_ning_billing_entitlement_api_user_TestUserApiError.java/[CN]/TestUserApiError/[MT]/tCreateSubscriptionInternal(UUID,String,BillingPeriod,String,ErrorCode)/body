{
  try {
    entitlementApi.createSubscription(bundleId,getProductSpecifier(productName,planSet,term,null),clock.getUTCNow());
    assertFalse(true);
  }
 catch (  EntitlementUserApiException e) {
    assertEquals(e.getCode(),expected.getCode());
    try {
      log.info(e.getMessage());
    }
 catch (    Throwable el) {
      assertFalse(true);
    }
  }
}
