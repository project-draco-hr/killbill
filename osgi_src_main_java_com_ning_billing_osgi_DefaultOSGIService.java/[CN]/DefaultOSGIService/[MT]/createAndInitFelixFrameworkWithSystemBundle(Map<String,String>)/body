{
  final Map<Object,Object> felixConfig=new HashMap<Object,Object>();
  felixConfig.putAll(config);
  felixConfig.put(FelixConstants.SYSTEMBUNDLE_ACTIVATORS_PROP,ImmutableList.<BundleActivator>of(killbillActivator,new Activator(),new OsgiManagerActivator(),new org.apache.felix.log.Activator(),new ConfigurationManager()));
  final Framework felix=new Felix(felixConfig);
  felix.init();
  return felix;
}
