{
  return !(InvoiceItemType.CBA_ADJ.equals(invoiceItem.getInvoiceItemType()) && (otherInvoiceItemsOnInvoice.size() == 1 && InvoiceItemType.CREDIT_ADJ.equals(otherInvoiceItemsOnInvoice.iterator().next().getInvoiceItemType()) && otherInvoiceItemsOnInvoice.iterator().next().getAmount().compareTo(invoiceItem.getAmount().negate()) == 0));
}
