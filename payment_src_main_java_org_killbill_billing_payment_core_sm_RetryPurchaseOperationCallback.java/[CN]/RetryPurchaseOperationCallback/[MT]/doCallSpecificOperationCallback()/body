{
  return directPaymentProcessor.createPurchase(retryableDirectPaymentStateContext.isApiPayment(),retryableDirectPaymentStateContext.getAccount(),retryableDirectPaymentStateContext.getPaymentMethodId(),retryableDirectPaymentStateContext.getDirectPaymentId(),retryableDirectPaymentStateContext.getAmount(),retryableDirectPaymentStateContext.getCurrency(),retryableDirectPaymentStateContext.getDirectPaymentExternalKey(),retryableDirectPaymentStateContext.getDirectPaymentTransactionExternalKey(),false,retryableDirectPaymentStateContext.getProperties(),retryableDirectPaymentStateContext.getCallContext(),retryableDirectPaymentStateContext.getInternalCallContext());
}
