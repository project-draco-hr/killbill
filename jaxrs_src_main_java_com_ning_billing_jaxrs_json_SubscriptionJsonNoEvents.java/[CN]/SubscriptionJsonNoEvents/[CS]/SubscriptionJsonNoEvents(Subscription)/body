{
  this(data.getId().toString(),data.getBundleId().toString(),data.getStartDate(),data.getCurrentPlan().getProduct().getName(),data.getCurrentPlan().getProduct().getCategory().toString(),data.getCurrentPlan().getBillingPeriod().toString(),data.getCurrentPriceList().getName(),data.getChargedThroughDate(),data.getEndDate() != null ? data.getEndDate() : data.getFutureEndDate());
}
