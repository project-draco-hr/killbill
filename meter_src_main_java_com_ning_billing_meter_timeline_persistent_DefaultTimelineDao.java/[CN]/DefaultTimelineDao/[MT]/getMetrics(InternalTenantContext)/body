{
  final HashBiMap<Integer,CategoryRecordIdAndMetric> accumulator=HashBiMap.create();
  for (  final Map<String,Object> metricInfo : delegate.getMetrics(context)) {
    accumulator.put(Integer.valueOf(metricInfo.get("record_id").toString()),new CategoryRecordIdAndMetric((Integer)metricInfo.get("category_record_id"),metricInfo.get("metric").toString()));
  }
  return accumulator;
}
