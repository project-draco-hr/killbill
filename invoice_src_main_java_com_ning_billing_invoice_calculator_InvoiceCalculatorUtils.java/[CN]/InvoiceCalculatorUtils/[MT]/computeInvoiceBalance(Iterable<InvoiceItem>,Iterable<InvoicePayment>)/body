{
  return computeInvoiceAmountCharged(invoiceItems).add(computeInvoiceAmountCredited(invoiceItems)).add(computeInvoiceAmountAdjustedForAccountCredit(invoiceItems)).add(computeInvoiceAmountPaid(invoicePayments).negate().add(computeInvoiceAmountRefunded(invoicePayments).negate())).setScale(NUMBER_OF_DECIMALS,ROUNDING_METHOD);
}
