{
  final OnSuccessPaymentControlResult result=controlPluginRunner.executePluginOnSuccessCalls(paymentStateContext.getAccount(),paymentControlContext.getPaymentMethodId(),paymentStateControlContext.getAttemptId(),paymentStateContext.getPaymentId(),paymentStateContext.getPaymentExternalKey(),paymentStateContext.getPaymentTransactionExternalKey(),PaymentApiType.PAYMENT_TRANSACTION,paymentStateContext.getTransactionType(),null,paymentControlContext.getAmount(),paymentControlContext.getCurrency(),paymentStateControlContext.isApiPayment(),paymentControlPluginNames,paymentStateContext.getProperties(),paymentStateContext.getCallContext());
  adjustStateContextPluginProperties(paymentStateContext,result.getAdjustedPluginProperties());
}
