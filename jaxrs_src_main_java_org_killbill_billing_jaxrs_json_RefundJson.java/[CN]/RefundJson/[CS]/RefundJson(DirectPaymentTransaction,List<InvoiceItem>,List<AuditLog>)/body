{
  this(refund.getId().toString(),refund.getDirectPaymentId().toString(),refund.getAmount(),refund.getCurrency().toString(),null,false,refund.getCreatedDate(),refund.getCreatedDate(),adjustments == null ? null : ImmutableList.<InvoiceItemJson>copyOf(Collections2.transform(adjustments,new Function<InvoiceItem,InvoiceItemJson>(){
    @Override public InvoiceItemJson apply(    @Nullable final InvoiceItem input){
      return new InvoiceItemJson(input);
    }
  }
)),toAuditLogJson(auditLogs));
}
