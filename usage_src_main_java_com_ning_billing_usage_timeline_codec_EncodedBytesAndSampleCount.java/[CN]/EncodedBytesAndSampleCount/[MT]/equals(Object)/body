{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  final EncodedBytesAndSampleCount that=(EncodedBytesAndSampleCount)o;
  if (sampleCount != that.sampleCount) {
    return false;
  }
  if (!Arrays.equals(encodedBytes,that.encodedBytes)) {
    return false;
  }
  return true;
}
