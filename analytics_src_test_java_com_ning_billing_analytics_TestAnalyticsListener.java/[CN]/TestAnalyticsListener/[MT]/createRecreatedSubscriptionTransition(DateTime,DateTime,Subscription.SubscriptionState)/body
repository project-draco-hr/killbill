{
  final ApiEventType eventType=ApiEventType.RE_CREATE;
  final Subscription.SubscriptionState nextState=Subscription.SubscriptionState.ACTIVE;
  return new SubscriptionTransitionData(UUID.randomUUID(),subscriptionId,bundleUUID,EntitlementEvent.EventType.API_USER,eventType,requestedTransitionTime,effectiveTransitionTime,previousState,null,null,null,nextState,plan,phase,priceList,1L,true);
