{
  final ApiEventType eventType=ApiEventType.CREATE;
  final ISubscription.SubscriptionState nextState=ISubscription.SubscriptionState.ACTIVE;
  return new SubscriptionTransition(UUID.randomUUID(),subscriptionId,bundleUUID,IEntitlementEvent.EventType.API_USER,eventType,requestedTransitionTime,effectiveTransitionTime,null,null,null,null,nextState,plan,phase,priceList);
}
