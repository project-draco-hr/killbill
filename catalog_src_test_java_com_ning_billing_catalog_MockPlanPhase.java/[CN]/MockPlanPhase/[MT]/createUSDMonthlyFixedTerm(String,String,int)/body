{
  return new MockPlanPhase(BillingPeriod.MONTHLY,PhaseType.FIXEDTERM,new DefaultDuration().setUnit(TimeUnit.MONTHS).setNumber(durationInMonths),(reccuringUSDPrice == null) ? null : MockInternationalPrice.createUSD(reccuringUSDPrice),(fixedPrice == null) ? null : MockInternationalPrice.createUSD(fixedPrice));
}
