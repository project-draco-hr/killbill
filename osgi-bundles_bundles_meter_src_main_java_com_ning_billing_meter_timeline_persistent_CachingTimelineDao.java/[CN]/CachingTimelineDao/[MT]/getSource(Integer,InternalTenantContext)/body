{
  String result=sourcesCache.get(sourceId);
  if (result == null) {
    result=delegate.getSource(sourceId,context);
    if (result != null) {
synchronized (sourcesCache) {
        if (sourcesCache.get(sourceId) == null) {
          sourcesCache.put(sourceId,result);
        }
      }
    }
  }
  return result;
}
