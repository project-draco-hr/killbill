{
  final DataInputStream inputStream=new DataInputStream(new ByteArrayInputStream(timesAndSamples));
  try {
    return inputStream.readInt();
  }
 catch (  IOException e) {
    throw new IllegalStateException(String.format("Exception reading timeByteCount in TimelineChunkMapper.map() for timesAndSamples %s",new String(Hex.encodeHex(timesAndSamples))),e);
  }
}
