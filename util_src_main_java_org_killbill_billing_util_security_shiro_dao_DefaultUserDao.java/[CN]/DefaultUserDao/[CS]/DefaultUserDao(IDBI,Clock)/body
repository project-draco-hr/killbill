{
  this.dbi=dbi;
  this.clock=clock;
  ((DBI)dbi).registerMapper(new LowerToCamelBeanMapperFactory(UserModelDao.class));
  ((DBI)dbi).registerMapper(new LowerToCamelBeanMapperFactory(UserRolesModelDao.class));
