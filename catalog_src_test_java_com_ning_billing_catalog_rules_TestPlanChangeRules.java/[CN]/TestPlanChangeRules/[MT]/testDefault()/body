{
  Product P1=createProduct("FP");
  Product P2=createProduct("BP");
  Catalog c=createCatalog(new PlanPolicyChangeRule[]{new PlanPolicyChangeRule(Qualifier.DEFAULT,ActionPolicy.END_OF_TERM,null)},null,null,P1,P2);
  assertEquals(ActionPolicy.END_OF_TERM,c.getPlanChangePolicy(new PlanPhaseSpecifier("FP",null,ANNUAL,null,EVERGREEN),new PlanSpecifier("BP",null,ANNUAL,null)));
}
