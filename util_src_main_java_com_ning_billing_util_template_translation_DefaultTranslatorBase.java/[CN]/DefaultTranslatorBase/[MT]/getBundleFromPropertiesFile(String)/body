{
  try {
    final InputStream inputStream=UriAccessor.accessUri(propertiesFileName);
    return new PropertyResourceBundle(inputStream);
  }
 catch (  MissingResourceException mrex) {
    return null;
  }
catch (  URISyntaxException e) {
    return null;
  }
catch (  IOException e) {
    return null;
  }
}
