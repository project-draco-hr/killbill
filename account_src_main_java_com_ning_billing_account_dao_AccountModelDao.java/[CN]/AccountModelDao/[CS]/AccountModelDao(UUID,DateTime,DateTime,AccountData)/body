{
  this(id,createdDate,updatedDate,account.getExternalKey(),account.getEmail(),account.getName(),account.getFirstNameLength(),account.getCurrency(),account.getBillCycleDay() == null ? 0 : account.getBillCycleDay().getDayOfMonthLocal(),account.getBillCycleDay() == null ? 0 : account.getBillCycleDay().getDayOfMonthUTC(),account.getPaymentMethodId(),account.getTimeZone(),account.getLocale(),account.getAddress1(),account.getAddress2(),account.getCompanyName(),account.getCity(),account.getStateOrProvince(),account.getCountry(),account.getPostalCode(),account.getPhone(),account.isMigrated(),account.isNotifiedForInvoices());
}
