{
  return new DefaultSubscriptionEvent(in.getId(),in.getSubscriptionId(),in.getEffectiveTransitionTime(),eventType,false,false,getServiceName(eventType),eventType.toString(),(in.getPreviousPlan() != null ? in.getPreviousPlan().getProduct() : null),in.getPreviousPlan(),in.getPreviousPhase(),in.getPreviousPriceList(),(in.getPreviousPlan() != null ? in.getPreviousPlan().getRecurringBillingPeriod() : null),(in.getNextPlan() != null ? in.getNextPlan().getProduct() : null),in.getNextPlan(),in.getNextPhase(),in.getNextPriceList(),(in.getNextPlan() != null ? in.getNextPlan().getRecurringBillingPeriod() : null),in.getCreatedDate(),referenceTime,accountTimeZone,internalTenantContext);
}
