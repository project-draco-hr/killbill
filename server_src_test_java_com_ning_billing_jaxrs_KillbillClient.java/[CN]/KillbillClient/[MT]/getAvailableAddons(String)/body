{
  final Response response=doGet(JaxrsResource.CATALOG_PATH + "/availableAddons",ImmutableMap.<String,String>of("baseProductName",baseProductName),DEFAULT_HTTP_TIMEOUT_SEC);
  Assert.assertEquals(response.getStatusCode(),Status.OK.getStatusCode());
  final String body=response.getResponseBody();
  return mapper.readValue(body,new TypeReference<List<PlanDetailJson>>(){
  }
);
}
