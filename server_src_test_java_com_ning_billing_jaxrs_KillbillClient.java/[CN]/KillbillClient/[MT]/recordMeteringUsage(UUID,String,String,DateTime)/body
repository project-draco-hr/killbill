{
  final String meterURI=JaxrsResource.METER_PATH + "/" + bundleId.toString()+ "/"+ category+ "/"+ metric;
  final Response meterResponse=doPost(meterURI,null,ImmutableMap.<String,String>of(JaxrsResource.QUERY_METER_WITH_AGGREGATE,"true",JaxrsResource.QUERY_METER_TIMESTAMP,timestamp.toString()),DEFAULT_HTTP_TIMEOUT_SEC);
  assertEquals(meterResponse.getStatusCode(),Status.OK.getStatusCode());
}
