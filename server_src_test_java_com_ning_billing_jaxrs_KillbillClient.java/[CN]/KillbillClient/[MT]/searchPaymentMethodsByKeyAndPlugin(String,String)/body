{
  final String uri=JaxrsResource.PAYMENT_METHODS_PATH + "/" + JaxrsResource.SEARCH+ "/"+ key;
  final Response response=doGet(uri,pluginName == null ? DEFAULT_EMPTY_QUERY : ImmutableMap.<String,String>of(JaxrsResource.QUERY_PAYMENT_METHOD_PLUGIN_NAME,pluginName),DEFAULT_HTTP_TIMEOUT_SEC);
  Assert.assertEquals(response.getStatusCode(),Status.OK.getStatusCode());
  final String baseJson=response.getResponseBody();
  final List<PaymentMethodJson> objFromJson=mapper.readValue(baseJson,new TypeReference<List<PaymentMethodJson>>(){
  }
);
  Assert.assertNotNull(objFromJson);
  return objFromJson;
}
