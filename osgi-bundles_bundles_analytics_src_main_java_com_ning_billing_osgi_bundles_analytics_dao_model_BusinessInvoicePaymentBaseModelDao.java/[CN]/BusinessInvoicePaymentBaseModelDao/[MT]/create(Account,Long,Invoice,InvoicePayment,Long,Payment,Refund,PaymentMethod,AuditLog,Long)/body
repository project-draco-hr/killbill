{
  if (invoicePayment.getType().equals(InvoicePaymentType.REFUND)) {
    return new BusinessInvoicePaymentRefundModelDao(account,accountRecordId,invoice,invoicePayment,invoicePaymentRecordId,payment,refund,paymentMethod,creationAuditLog,tenantRecordId);
  }
 else   if (invoicePayment.getType().equals(InvoicePaymentType.CHARGED_BACK)) {
    return new BusinessInvoicePaymentChargebackModelDao(account,accountRecordId,invoice,invoicePayment,invoicePaymentRecordId,payment,refund,paymentMethod,creationAuditLog,tenantRecordId);
  }
 else {
    return new BusinessInvoicePaymentModelDao(account,accountRecordId,invoice,invoicePayment,invoicePaymentRecordId,payment,refund,paymentMethod,creationAuditLog,tenantRecordId);
  }
}
