{
  final URI uri=UriBuilder.fromPath(objectId.toString()).build();
  final Response.ResponseBuilder ri=Response.created(uri);
  return ri.entity(new Object(){
    @SuppressWarnings(value="all") public URI getUri(){
      final URI newUriFromResource=objectId != null ? UriBuilder.fromResource(theClass).path(theClass,getMethodName).build(objectId) : UriBuilder.fromResource(theClass).path(theClass,getMethodName).build();
      return newUriFromResource;
    }
  }
).build();
}
