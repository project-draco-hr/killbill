{
  return ImmutableList.<Invoice>copyOf(Collections2.transform(dao.getInvoicesByAccount(accountId,fromDate,internalCallContextFactory.createInternalTenantContext(context)),new Function<InvoiceModelDao,Invoice>(){
    @Override public Invoice apply(    final InvoiceModelDao input){
      return new DefaultInvoice(input);
    }
  }
));
}
