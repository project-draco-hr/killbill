{
  return ImmutableList.<Invoice>copyOf(Collections2.transform(dao.getInvoicesByAccount(fromDate,internalCallContextFactory.createInternalTenantContext(accountId,context)),new Function<InvoiceModelDao,Invoice>(){
    @Override public Invoice apply(    final InvoiceModelDao input){
      return new DefaultInvoice(input);
    }
  }
));
}
