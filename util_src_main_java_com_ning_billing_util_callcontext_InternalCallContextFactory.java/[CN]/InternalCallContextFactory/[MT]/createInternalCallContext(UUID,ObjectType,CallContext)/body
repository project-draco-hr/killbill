{
  final Long tenantRecordIdFromObject=retrieveTenantRecordIdFromObject(objectId,objectType);
  final Long tenantRecordIdFromContext=getTenantRecordId(context);
  return createInternalCallContext(objectId,objectType,context.getUserName(),context.getCallOrigin(),context.getUserType(),context.getUserToken(),context.getReasonCode(),context.getComment(),context.getCreatedDate(),context.getUpdatedDate());
}
