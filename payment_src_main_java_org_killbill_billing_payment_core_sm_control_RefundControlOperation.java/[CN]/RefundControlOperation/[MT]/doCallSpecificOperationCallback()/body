{
  return paymentProcessor.createRefund(paymentStateControlContext.isApiPayment(),paymentStateControlContext.getAttemptId(),paymentStateControlContext.getAccount(),paymentStateControlContext.getPaymentId(),paymentStateControlContext.getAmount(),paymentStateControlContext.getCurrency(),paymentStateControlContext.getPaymentTransactionExternalKey(),false,paymentStateControlContext.getProperties(),paymentStateControlContext.getCallContext(),paymentStateControlContext.getInternalCallContext());
}
