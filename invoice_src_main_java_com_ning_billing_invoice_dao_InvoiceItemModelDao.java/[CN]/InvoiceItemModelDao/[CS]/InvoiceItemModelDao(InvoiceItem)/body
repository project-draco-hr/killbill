{
  this(invoiceItem.getInvoiceItemType(),invoiceItem.getId(),invoiceItem.getAccountId(),invoiceItem.getBundleId(),invoiceItem.getSubscriptionId(),invoiceItem.getPlanName(),invoiceItem.getPhaseName(),invoiceItem.getStartDate(),invoiceItem.getEndDate(),invoiceItem.getAmount(),invoiceItem.getRate(),invoiceItem.getCurrency(),invoiceItem.getLinkedItemId());
}
