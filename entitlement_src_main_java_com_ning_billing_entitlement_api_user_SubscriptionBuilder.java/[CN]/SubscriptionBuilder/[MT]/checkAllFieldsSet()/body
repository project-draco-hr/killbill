{
  for (  final Field cur : SubscriptionBuilder.class.getDeclaredFields()) {
    try {
      final Object value=cur.get(this);
      if (value == null) {
        throw new EntitlementError(String.format("Field %s has not been set for Subscription",cur.getName()));
      }
