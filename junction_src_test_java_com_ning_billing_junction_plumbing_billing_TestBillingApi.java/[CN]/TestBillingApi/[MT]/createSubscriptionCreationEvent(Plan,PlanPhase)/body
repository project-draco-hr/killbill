{
  final DateTime now=clock.getUTCNow();
  final DateTime then=now.minusDays(1);
  final PriceList nextPriceList=catalog.findPriceList(PriceListSet.DEFAULT_PRICELIST_NAME,now);
  final EffectiveSubscriptionInternalEvent t=new MockEffectiveSubscriptionEvent(eventId,subId,bunId,then,now,null,null,null,null,SubscriptionState.ACTIVE,nextPlan.getName(),nextPhase.getName(),nextPriceList.getName(),1L,null,SubscriptionTransitionType.CREATE,0,null);
  effectiveSubscriptionTransitions.add(t);
  return now;
}
