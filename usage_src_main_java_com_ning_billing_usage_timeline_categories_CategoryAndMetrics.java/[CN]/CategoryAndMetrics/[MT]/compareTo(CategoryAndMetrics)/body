{
  final int categoryComparison=eventCategory.compareTo(o.getEventCategory());
  if (categoryComparison != 0) {
    return categoryComparison;
  }
 else {
    if (metrics.size() > o.getMetrics().size()) {
      return 1;
    }
 else     if (metrics.size() < o.getMetrics().size()) {
      return -1;
    }
 else {
      return 0;
    }
  }
}
