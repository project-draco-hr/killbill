{
  final PersistentBusConfig busConfig=new ConfigurationObjectFactory(configSource).buildWithReplacements(PersistentBusConfig.class,ImmutableMap.<String,String>of("instanceName","main"));
  bind(BusProvider.class).toInstance(new BusProvider(busConfig));
  bind(PersistentBus.class).toProvider(BusProvider.class).asEagerSingleton();
}
