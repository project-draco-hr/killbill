{
  try {
    factory.createOverdueWrapperFor(blockable).refresh();
  }
 catch (  OverdueException e) {
    log.error("Error processing Overdue for Blockable with id: " + blockable.getId().toString(),e);
  }
catch (  OverdueApiException e) {
    log.error("Error processing Overdue for Blockable with id: " + blockable.getId().toString(),e);
  }
}
