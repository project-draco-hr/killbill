{
  Notification res=dbi.withHandle(new HandleCallback<Notification>(){
    @Override public Notification withHandle(    Handle handle) throws Exception {
      Notification res=handle.createQuery("   select" + " record_id " + ", id"+ ", notification_key"+ ", created_date"+ ", effective_date"+ ", queue_name"+ ", processing_owner"+ ", processing_available_date"+ ", processing_state"+ "    from notifications "+ " where "+ " id = '" + notificationId + "';").map(new NotificationSqlMapper()).first();
      return res;
    }
  }
);
  return res;
}
