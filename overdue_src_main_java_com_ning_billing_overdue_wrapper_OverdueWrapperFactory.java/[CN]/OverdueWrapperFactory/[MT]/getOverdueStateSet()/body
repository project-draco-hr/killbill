{
  if (config == null || config.getStateSet() == null) {
    return new DefaultOverdueStateSet(){
      @SuppressWarnings("unchecked") @Override protected DefaultOverdueState[] getStates(){
        return new DefaultOverdueState[0];
      }
      @Override public Period getInitialReevaluationInterval(){
        return null;
      }
    }
;
  }
 else {
    return config.getStateSet();
  }
}
