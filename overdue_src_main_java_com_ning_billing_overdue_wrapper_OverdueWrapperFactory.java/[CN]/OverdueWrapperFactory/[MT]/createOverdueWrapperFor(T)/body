{
  if (bloackable instanceof SubscriptionBundle) {
    return (OverdueWrapper<T>)new OverdueWrapper<SubscriptionBundle>((SubscriptionBundle)bloackable,api,getOverdueStateSetBundle(),clock,billingStateCalcuatorBundle,overdueStateApplicatorBundle);
  }
 else {
    throw new OverdueException(ErrorCode.OVERDUE_TYPE_NOT_SUPPORTED,bloackable.getId(),bloackable.getClass());
  }
}
