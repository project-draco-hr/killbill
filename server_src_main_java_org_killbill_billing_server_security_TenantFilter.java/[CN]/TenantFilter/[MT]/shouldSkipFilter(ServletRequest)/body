{
  boolean shouldSkip=false;
  if (request instanceof HttpServletRequest) {
    final HttpServletRequest httpServletRequest=(HttpServletRequest)request;
    final String path=httpServletRequest.getRequestURI();
    if (("/1.0/kb/tenants".equals(path) && "POST".equals(httpServletRequest.getMethod())) || (KillbillGuiceListener.METRICS_SERVLETS_PATHS.contains(path) && "GET".equals(httpServletRequest.getMethod())) || (!path.startsWith("/1.0") && "GET".equals(httpServletRequest.getMethod()))) {
      shouldSkip=true;
    }
  }
  return shouldSkip;
}
