{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  final SourceRecordIdAndMetricRecordId that=(SourceRecordIdAndMetricRecordId)o;
  if (metricId != that.metricId) {
