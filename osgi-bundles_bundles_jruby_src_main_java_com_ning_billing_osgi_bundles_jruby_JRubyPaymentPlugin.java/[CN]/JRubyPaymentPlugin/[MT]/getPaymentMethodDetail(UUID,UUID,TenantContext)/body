{
  return callWithRuntimeAndChecking(new PluginCallback(){
    @Override public PaymentMethodPlugin doCall(    final Ruby runtime){
      final IRubyObject res=pluginInstance.callMethod("get_payment_method_detail",JavaEmbedUtils.javaToRuby(runtime,kbAccountId.toString()),JavaEmbedUtils.javaToRuby(runtime,kbPaymentMethodId.toString()));
      return null;
    }
  }
);
}
