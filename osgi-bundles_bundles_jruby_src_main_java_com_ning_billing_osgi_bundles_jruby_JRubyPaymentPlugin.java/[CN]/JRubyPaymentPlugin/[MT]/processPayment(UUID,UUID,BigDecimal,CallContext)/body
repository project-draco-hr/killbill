{
  return callWithRuntimeAndChecking(new PluginCallback(){
    @Override public PaymentInfoPlugin doCall(    final Ruby runtime){
      final IRubyObject res=pluginInstance.callMethod("charge",JavaEmbedUtils.javaToRuby(runtime,kbPaymentId.toString()),JavaEmbedUtils.javaToRuby(runtime,kbPaymentMethodId.toString()),JavaEmbedUtils.javaToRuby(runtime,amount.longValue() * 100));
      return null;
    }
  }
);
}
