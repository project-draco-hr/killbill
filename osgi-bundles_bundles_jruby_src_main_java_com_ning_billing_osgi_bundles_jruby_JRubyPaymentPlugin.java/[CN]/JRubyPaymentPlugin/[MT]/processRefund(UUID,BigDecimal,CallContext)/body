{
  return callWithRuntimeAndChecking(new PluginCallback(){
    @Override public RefundInfoPlugin doCall(    final Ruby runtime){
      final IRubyObject res=pluginInstance.callMethod("refund",JavaEmbedUtils.javaToRuby(runtime,kbPaymentId.toString()),JavaEmbedUtils.javaToRuby(runtime,refundAmount.longValue() * 100));
      return null;
    }
  }
);
}
