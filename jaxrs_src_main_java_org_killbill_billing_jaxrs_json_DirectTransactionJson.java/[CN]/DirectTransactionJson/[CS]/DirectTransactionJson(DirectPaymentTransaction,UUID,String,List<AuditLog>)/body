{
  this(dpt.getId().toString(),directPaymentId.toString(),dpt.getTransactionType().toString(),dpt.getAmount(),dpt.getCurrency() != null ? dpt.getCurrency().toString() : null,dpt.getEffectiveDate(),dpt.getPaymentStatus() != null ? dpt.getPaymentStatus().toString() : null,1,externalKey,dpt.getGatewayErrorCode(),dpt.getGatewayErrorMsg(),toAuditLogJson(directTransactionLogs));
}
