{
  this(dpt.getId().toString(),directPaymentId.toString(),dpt.getTransactionType().toString(),dpt.getAmount(),dpt.getCurrency().toString(),dpt.getEffectiveDate(),dpt.getPaymentStatus().toString(),1,externalKey,dpt.getGatewayErrorCode(),dpt.getGatewayErrorMsg(),toAuditLogJson(directTransactionLogs));
