{
  this(src.getId(),src.getCreatedDate(),src.getUpdatedDate(),src.getAccountId(),src.getInvoiceId(),src.getPaymentMethodId(),src.getAmount(),toPaidAmount(src.getPaymentStatus(),src.getAmount(),refunds),src.getCurrency(),src.getEffectiveDate(),src.getPaymentNumber(),src.getPaymentStatus(),null,paymentPluginInfo,src.getExtFirstPaymentRefId(),src.getExtSecondPaymentRefId(),toPaymentAttempts(attempts));
