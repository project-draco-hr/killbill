{
  return directPaymentProcessor.createRefund(retryableDirectPaymentStateContext.isApiPayment(),retryableDirectPaymentStateContext.getAttemptId(),retryableDirectPaymentStateContext.getAccount(),retryableDirectPaymentStateContext.getDirectPaymentId(),retryableDirectPaymentStateContext.getAmount(),retryableDirectPaymentStateContext.getCurrency(),retryableDirectPaymentStateContext.getDirectPaymentTransactionExternalKey(),false,retryableDirectPaymentStateContext.getProperties(),retryableDirectPaymentStateContext.getCallContext(),retryableDirectPaymentStateContext.getInternalCallContext());
}
