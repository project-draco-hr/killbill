{
  final List<String> tags=new ArrayList<String>();
  Iterables.addAll(tags,splitter.split(r.getString(5)));
  final BusinessAccount account=new BusinessAccount(r.getString(1),BigDecimal.valueOf(r.getDouble(4)),tags,new DateTime(r.getLong(6),DateTimeZone.UTC),BigDecimal.valueOf(r.getDouble(7)),r.getString(8),r.getString(9),r.getString(10),r.getString(11));
  account.setCreatedDt(new DateTime(r.getLong(2),DateTimeZone.UTC));
  account.setUpdatedDt(new DateTime(r.getLong(3),DateTimeZone.UTC));
  return account;
}
