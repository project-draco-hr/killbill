{
  final BusinessAccount account=new BusinessAccount(UUID.fromString(r.getString(1)),r.getString(2),r.getString(6),BigDecimal.valueOf(r.getDouble(5)),r.getLong(7) == 0 ? null : new DateTime(r.getLong(7),DateTimeZone.UTC),BigDecimal.valueOf(r.getDouble(8)),r.getString(9),r.getString(10),r.getString(11),r.getString(12));
  account.setCreatedDt(new DateTime(r.getLong(3),DateTimeZone.UTC));
  account.setUpdatedDt(new DateTime(r.getLong(4),DateTimeZone.UTC));
  return account;
}
