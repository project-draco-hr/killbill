{
  final BusinessAccount account=new BusinessAccount(UUID.fromString(r.getString(1)),r.getString(2),r.getString(6),BigDecimal.valueOf(r.getDouble(5)),getDate(r,"last_invoice_date"),BigDecimal.valueOf(r.getDouble(8)),r.getString(9),r.getString(10),r.getString(11),r.getString(12),r.getString(13));
  account.setCreatedDt(new DateTime(r.getLong(3),DateTimeZone.UTC));
  account.setUpdatedDt(new DateTime(r.getLong(4),DateTimeZone.UTC));
  return account;
}
