  LOG.debug("Java " + System.getProperty("java.version") + " ("+ System.getProperty("java.vendor")+ ")");
  LOG.debug(System.getProperty("os.name") + " " + System.getProperty("os.version")+ " "+ System.getProperty("os.arch")+ "\n");
}
