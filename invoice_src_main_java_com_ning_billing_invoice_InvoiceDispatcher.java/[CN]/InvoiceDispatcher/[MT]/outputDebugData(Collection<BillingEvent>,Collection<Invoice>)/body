{
  if (VERBOSE_OUTPUT) {
    log.info("Events");
    for (    final BillingEvent event : events) {
      log.info(event.toString());
    }
    log.info("Existing items");
    for (    final Invoice invoice : invoices) {
      for (      final InvoiceItem item : invoice.getInvoiceItems()) {
        log.info(item.toString());
      }
    }
  }
}
