{
  final String json=objectMapper.writeValueAsString(notificationKey);
  final UUID futureUserToken=UUID.randomUUID();
  final Notification notification=new DefaultNotification(getFullQName(),hostname,notificationKey.getClass().getName(),json,context.getUserToken(),futureUserToken,futureNotificationTime,context.getAccountRecordId(),context.getTenantRecordId());
  thisDao.insertNotification(notification,context);
}
