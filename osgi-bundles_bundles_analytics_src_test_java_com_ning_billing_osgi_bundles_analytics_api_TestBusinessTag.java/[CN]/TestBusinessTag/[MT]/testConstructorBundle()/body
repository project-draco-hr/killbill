{
  final BusinessBundleTagModelDao businessBundleTagModelDao=new BusinessBundleTagModelDao(account,accountRecordId,tag,tagRecordId,tagDefinition,auditLog,tenantRecordId,reportGroup);
  final BusinessTag businessTag=BusinessTag.create(businessBundleTagModelDao);
  verifyBusinessTag(businessTag);
  Assert.assertEquals(businessTag.getObjectType(),ObjectType.BUNDLE);
}
