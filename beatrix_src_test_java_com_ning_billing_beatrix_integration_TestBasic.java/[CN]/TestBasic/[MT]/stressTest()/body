{
  final int maxIterations=3;
  int curIteration=maxIterations;
  for (curIteration=0; curIteration < maxIterations; curIteration++) {
    log.info("################################  ITERATION " + curIteration + "  #########################");
    setupTest();
    Thread.sleep(1000);
    testBasePlanCompleteWithBillingDayPresent();
  }
}
