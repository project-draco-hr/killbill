{
  try {
    blockingApi.setBlockingState(new BlockingState(blockable.getId(),nextOverdueState.getName(),Blockable.Type.get(blockable),OverdueService.OVERDUE_SERVICE_NAME,blockChanges(nextOverdueState),blockEntitlement(nextOverdueState),blockBilling(nextOverdueState)));
  }
 catch (  Exception e) {
    throw new OverdueError(e,ErrorCode.OVERDUE_CAT_ERROR_ENCOUNTERED,blockable.getId(),blockable.getClass().getName());
  }
}
