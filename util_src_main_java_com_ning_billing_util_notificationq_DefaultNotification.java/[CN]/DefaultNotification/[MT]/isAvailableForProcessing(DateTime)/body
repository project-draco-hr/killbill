{
switch (lifecycleState) {
case AVAILABLE:
    break;
case IN_PROCESSING:
  if (nextAvailableDate.isAfter(now)) {
    return false;
  }
break;
case PROCESSED:
return false;
default :
throw new RuntimeException(String.format("Unkwnon IEvent processing state %s",lifecycleState));
}
return effectiveDate.isBefore(now);
}
