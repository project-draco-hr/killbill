{
switch (policy) {
case IMMEDIATE:
    return clock.getUTCNow();
case END_OF_TERM:
  return (chargedThroughDate != null && chargedThroughDate.isAfter(clock.getUTCNow())) ? chargedThroughDate : clock.getUTCNow();
default :
throw new SubscriptionBaseError(String.format("Unexpected policy type %s",policy.toString()));
}
}
