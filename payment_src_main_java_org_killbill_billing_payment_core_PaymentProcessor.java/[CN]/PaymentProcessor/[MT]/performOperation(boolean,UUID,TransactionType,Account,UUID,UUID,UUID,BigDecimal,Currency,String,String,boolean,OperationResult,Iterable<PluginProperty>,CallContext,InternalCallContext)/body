{
  final UUID nonNullPaymentId=paymentAutomatonRunner.run(isApiPayment,transactionType,account,attemptId,paymentMethodId,paymentId,transactionId,paymentExternalKey,paymentTransactionExternalKey,amount,currency,shouldLockAccountAndDispatch,overridePluginOperationResult,properties,callContext,internalCallContext);
  return getPayment(nonNullPaymentId,true,properties,callContext,internalCallContext);
}
