{
  try {
    return Response.status(Response.Status.OK).build();
  }
 catch (  IllegalArgumentException e) {
    return Response.status(Response.Status.BAD_REQUEST).entity(e.getMessage()).build();
  }
catch (  NullPointerException e) {
    return Response.status(Response.Status.BAD_REQUEST).entity(e.getMessage()).build();
  }
}
