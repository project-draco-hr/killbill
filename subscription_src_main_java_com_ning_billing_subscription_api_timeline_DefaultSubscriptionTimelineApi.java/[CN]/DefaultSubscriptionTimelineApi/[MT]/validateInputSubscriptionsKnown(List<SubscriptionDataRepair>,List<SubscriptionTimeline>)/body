{
  for (  final SubscriptionTimeline cur : input) {
    boolean found=false;
    for (    final Subscription s : subscriptions) {
      if (s.getId().equals(cur.getId())) {
        found=true;
        break;
      }
    }
    if (!found) {
      throw new SubscriptionRepairException(ErrorCode.SUB_REPAIR_UNKNOWN_SUBSCRIPTION,cur.getId());
    }
  }
}
