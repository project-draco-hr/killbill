{
  final UUID kbPaymentId=UUID.randomUUID();
  final BigDecimal amount=new BigDecimal("1.394810E-3");
  final DateTime effectiveDate=clock.getUTCNow().plusDays(1);
  final DateTime createdDate=clock.getUTCNow();
  final PaymentPluginStatus status=PaymentPluginStatus.UNDEFINED;
  final String error=UUID.randomUUID().toString();
  final DefaultNoOpPaymentInfoPlugin info=new DefaultNoOpPaymentInfoPlugin(kbPaymentId,amount,Currency.USD,effectiveDate,createdDate,status,error);
  Assert.assertEquals(info,info);
  final DefaultNoOpPaymentInfoPlugin sameInfo=new DefaultNoOpPaymentInfoPlugin(kbPaymentId,amount,Currency.USD,effectiveDate,createdDate,status,error);
  Assert.assertEquals(sameInfo,info);
  final DefaultNoOpPaymentInfoPlugin otherInfo=new DefaultNoOpPaymentInfoPlugin(kbPaymentId,amount,Currency.USD,effectiveDate,createdDate,status,UUID.randomUUID().toString());
  Assert.assertNotEquals(otherInfo,info);
}
