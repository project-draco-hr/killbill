{
  final DefaultAccountData data=new DefaultAccountData("dsfdsf","bobo",3,"bobo@yahoo.com",new DefaultBillCycleDay(12),"USD",UUID.randomUUID(),"UTC","US","21 avenue","","Gling","San Franciso","CA","94110","USA","4126789887",false,false);
  final DefaultAccountCreationEvent e=new DefaultAccountCreationEvent(data,UUID.randomUUID(),UUID.randomUUID());
  final String json=mapper.writeValueAsString(e);
  final Object obj=mapper.readValue(json,DefaultAccountCreationEvent.class);
  Assert.assertTrue(obj.equals(e));
}
