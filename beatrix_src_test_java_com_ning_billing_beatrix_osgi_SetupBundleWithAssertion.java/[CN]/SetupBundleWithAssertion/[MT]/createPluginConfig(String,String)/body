{
  return new PluginJavaConfig(){
    @Override public String getBundleJarPath(){
      return bundleTestResourcePath;
    }
    @Override public String getPluginName(){
      return bundleName;
    }
    @Override public PluginType getPluginType(){
      return PluginType.PAYMENT;
    }
    @Override public String getVersion(){
      return killbillVersion;
    }
    @Override public String getPluginVersionnedName(){
      return bundleName + "-" + killbillVersion;
    }
    @Override public File getPluginVersionRoot(){
      final StringBuilder tmp=new StringBuilder(config.getRootInstallationDir());
      tmp.append("/plugins/").append(PluginLanguage.JAVA.toString().toLowerCase()).append("/").append(bundleName).append("/").append(killbillVersion);
      final File result=new File(tmp.toString());
      return result;
    }
    @Override public PluginLanguage getPluginLanguage(){
      return PluginLanguage.JAVA;
    }
  }
;
}
