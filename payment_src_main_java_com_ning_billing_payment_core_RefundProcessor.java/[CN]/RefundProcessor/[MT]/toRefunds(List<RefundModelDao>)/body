{
  return new ArrayList<Refund>(Collections2.transform(in,new Function<RefundModelDao,Refund>(){
    @Override public Refund apply(    final RefundModelDao cur){
      return new DefaultRefund(cur.getId(),cur.getCreatedDate(),cur.getUpdatedDate(),cur.getPaymentId(),cur.getAmount(),cur.getCurrency(),cur.isAdjsuted(),cur.getCreatedDate());
    }
  }
));
}
