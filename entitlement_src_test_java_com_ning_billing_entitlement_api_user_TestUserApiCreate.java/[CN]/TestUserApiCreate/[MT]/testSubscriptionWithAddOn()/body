{
  log.info("Starting testSubscriptionWithAddOn");
  try {
    final String productName="Shotgun";
    final BillingPeriod term=BillingPeriod.ANNUAL;
    final String planSetName=PriceListSet.DEFAULT_PRICELIST_NAME;
    testListener.pushExpectedEvent(NextEvent.CREATE);
    final SubscriptionData subscription=(SubscriptionData)entitlementApi.createSubscription(bundle.getId(),getProductSpecifier(productName,planSetName,term,null),clock.getUTCNow(),context);
    assertNotNull(subscription);
    assertListenerStatus();
  }
 catch (  EntitlementUserApiException e) {
    Assert.fail(e.getMessage());
  }
}
