{
  DateTime result=subscriptionStartDate.toDateTime();
  for (  final PlanPhase phase : getAllPhases()) {
    if (phase.getRecurringPrice() == null || phase.getRecurringPrice().isZero()) {
      result=phase.getDuration().addToDateTime(result);
    }
 else {
      break;
    }
  }
  return result;
}
