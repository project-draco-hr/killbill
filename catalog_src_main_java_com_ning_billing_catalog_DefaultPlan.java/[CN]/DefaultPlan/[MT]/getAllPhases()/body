{
  int length=(initialPhases == null || initialPhases.length == 0) ? 1 : (initialPhases.length + 1);
  PlanPhase[] allPhases=new DefaultPlanPhase[length];
  int cnt=0;
  if (length > 1) {
    for (    PlanPhase cur : initialPhases) {
      allPhases[cnt++]=cur;
    }
  }
  allPhases[cnt++]=finalPhase;
  return allPhases;
}
