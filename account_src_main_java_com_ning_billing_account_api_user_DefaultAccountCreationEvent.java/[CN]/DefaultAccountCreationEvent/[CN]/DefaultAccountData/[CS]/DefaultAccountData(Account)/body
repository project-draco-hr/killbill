{
  this(d.getExternalKey() != null ? d.getExternalKey().toString() : null,d.getName(),d.getFirstNameLength(),d.getEmail(),d.getBillCycleDay(),d.getCurrency() != null ? d.getCurrency().name() : null,d.getPaymentProviderName(),d.getTimeZone() != null ? d.getTimeZone().getID() : null,d.getLocale(),d.getAddress1(),d.getAddress2(),d.getCompanyName(),d.getCity(),d.getStateOrProvince(),d.getPostalCode(),d.getCountry(),d.getPhone());
}
