{
synchronized (events) {
    final List<SubscriptionEvent> results=new LinkedList<SubscriptionEvent>();
    for (    final SubscriptionEvent cur : events) {
      if (cur.isActive() && cur.getEffectiveDate().isAfter(clock.getUTCNow()) && cur.getSubscriptionId().equals(subscriptionId)) {
        results.add(cur);
      }
    }
    return results;
  }
}
