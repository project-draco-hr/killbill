{
  boolean found=false;
  final Iterator<Subscription> it=subscriptions.iterator();
  while (it.hasNext()) {
    final Subscription cur=it.next();
    if (cur.getId().equals(subscription.getId())) {
      found=true;
      it.remove();
      break;
    }
  }
  if (found) {
    subscriptions.add(subscription);
  }
}
