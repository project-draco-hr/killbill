{
  this(refund.getId().toString(),refund.getPaymentId().toString(),refund.getRefundAmount(),refund.getCurrency().toString(),refund.getRefundStatus().toString(),refund.isAdjusted(),refund.getEffectiveDate(),refund.getEffectiveDate(),adjustments == null ? null : ImmutableList.<InvoiceItemJson>copyOf(Collections2.transform(adjustments,new Function<InvoiceItem,InvoiceItemJson>(){
    @Override public InvoiceItemJson apply(    @Nullable final InvoiceItem input){
      return new InvoiceItemJson(input);
    }
  }
)),toAuditLogJson(auditLogs));
}
