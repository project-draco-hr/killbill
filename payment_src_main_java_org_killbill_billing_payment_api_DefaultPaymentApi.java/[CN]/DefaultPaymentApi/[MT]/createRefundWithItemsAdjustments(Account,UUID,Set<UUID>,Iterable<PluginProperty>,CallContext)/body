{
  final Map<UUID,BigDecimal> invoiceItemIdsWithAmounts=new HashMap<UUID,BigDecimal>();
  for (  final UUID invoiceItemId : invoiceItemIds) {
    invoiceItemIdsWithAmounts.put(invoiceItemId,null);
  }
  return refundProcessor.createRefund(account,paymentId,null,true,invoiceItemIdsWithAmounts,properties,context,internalCallContextFactory.createInternalCallContext(account.getId(),context));
}
