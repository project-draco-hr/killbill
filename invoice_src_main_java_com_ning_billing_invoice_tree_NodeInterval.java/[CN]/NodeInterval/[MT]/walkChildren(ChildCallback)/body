{
  NodeInterval prevChild=null;
  NodeInterval curChild=leftChild;
  while (curChild != null) {
    boolean shouldBreak=callback.performActionOnChild(prevChild,curChild);
    if (shouldBreak) {
      return curChild;
    }
    prevChild=curChild;
    curChild=curChild.getRightSibling();
  }
  return prevChild == null ? curChild : prevChild;
}
