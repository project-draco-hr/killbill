{
  logger.debug("Starting PURCHASE for payment {} ({} {})",paymentStateContext.getPaymentId(),paymentStateContext.getAmount(),paymentStateContext.getCurrency());
  return plugin.purchasePayment(paymentStateContext.getAccount().getId(),paymentStateContext.getPaymentId(),paymentStateContext.getTransactionId(),paymentStateContext.getPaymentMethodId(),paymentStateContext.getAmount(),paymentStateContext.getCurrency(),paymentStateContext.getProperties(),paymentStateContext.getCallContext());
}
