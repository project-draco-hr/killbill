{
  final StringBuilder sb=new StringBuilder();
  sb.append("BusinessInvoice");
  sb.append("{invoiceId=").append(invoiceId);
  sb.append(", invoiceNumber=").append(invoiceNumber);
  sb.append(", invoiceDate=").append(invoiceDate);
