{
  final BillCycleDay billCycleDay=Mockito.mock(BillCycleDay.class);
  Mockito.when(billCycleDay.getDayOfMonthUTC()).thenReturn(1);
  final AccountData accountData=new MockAccountBuilder().name(UUID.randomUUID().toString()).firstNameLength(6).email(UUID.randomUUID().toString()).phone(UUID.randomUUID().toString()).migrated(false).isNotifiedForInvoices(false).externalKey(UUID.randomUUID().toString()).billingCycleDay(billCycleDay).currency(Currency.USD).paymentMethodId(UUID.randomUUID()).timeZone(DateTimeZone.forID("Europe/Paris")).build();
  assertNotNull(accountData);
  return accountData;
}
