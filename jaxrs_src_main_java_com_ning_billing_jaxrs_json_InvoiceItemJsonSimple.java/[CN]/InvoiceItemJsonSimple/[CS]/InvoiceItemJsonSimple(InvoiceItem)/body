{
  this(toString(item.getId()),toString(item.getInvoiceId()),toString(item.getLinkedItemId()),toString(item.getAccountId()),toString(item.getBundleId()),toString(item.getSubscriptionId()),item.getPlanName(),item.getPhaseName(),item.getDescription(),item.getStartDate(),item.getEndDate(),item.getAmount(),item.getCurrency(),null);
}
