{
  this(null,invoice.getId(),invoice.getInvoiceNumber(),invoice.getInvoiceDate(),invoice.getTargetDate(),invoice.getCurrency() == null ? null : invoice.getCurrency().toString(),invoice.getBalance(),invoice.getPaidAmount(),invoice.getChargedAmount(),null,invoice.getCreditAdjAmount(),invoice.getCreatedDate(),creationAuditLog.getUserName(),creationAuditLog.getReasonCode(),creationAuditLog.getComment(),account.getId(),account.getName(),account.getExternalKey(),null,null);
