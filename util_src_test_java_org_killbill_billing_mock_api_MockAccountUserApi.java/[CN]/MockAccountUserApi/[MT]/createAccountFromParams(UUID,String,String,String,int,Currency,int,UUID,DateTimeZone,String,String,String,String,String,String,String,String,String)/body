{
  final Account result=new MockAccountBuilder(id).externalKey(externalKey).email(email).name(name).firstNameLength(firstNameLength).currency(currency).billingCycleDayLocal(billCycleDayLocal).paymentMethodId(paymentMethodId).timeZone(timeZone).locale(locale).address1(address1).address2(address2).companyName(companyName).city(city).stateOrProvince(stateOrProvince).country(country).postalCode(postalCode).phone(phone).isNotifiedForInvoices(false).build();
  accounts.add(result);
  return result;
}
