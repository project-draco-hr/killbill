{
  try {
    Preconditions.checkNotNull(createdBy,String.format("Header %s needs to be set",HDR_CREATED_BY));
    final Tenant tenant=getTenantFromRequest(request);
    return contextFactory.createCallContext(tenant == null ? null : tenant.getId(),createdBy,CallOrigin.EXTERNAL,UserType.CUSTOMER,reason,comment,UUID.randomUUID());
  }
 catch (  NullPointerException e) {
    throw new IllegalArgumentException(e.getMessage());
  }
}
