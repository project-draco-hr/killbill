{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  final RepeatSample that=(RepeatSample)o;
  if (repeatCount != that.repeatCount) {
    return false;
  }
  if (sampleRepeated != null ? !sampleRepeated.equals(that.sampleRepeated) : that.sampleRepeated != null) {
    return false;
  }
  return true;
}
