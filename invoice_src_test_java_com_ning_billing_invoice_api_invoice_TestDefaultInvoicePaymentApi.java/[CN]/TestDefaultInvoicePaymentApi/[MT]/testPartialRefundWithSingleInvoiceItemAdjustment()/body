{
  final Invoice invoice=createAndPersistInvoice(invoiceSqlDao,invoiceItemSqlDao,clock,ImmutableList.<BigDecimal>of(THIRTY,BigDecimal.TEN),CURRENCY,callContext,internalCallContextFactory);
  final Map<UUID,BigDecimal> adjustments=new HashMap<UUID,BigDecimal>();
  adjustments.put(invoice.getInvoiceItems().get(0).getId(),null);
  verifyRefund(invoice,new BigDecimal("40"),new BigDecimal("30"),BigDecimal.ZERO,true,adjustments);
}
