{
  return paymentProcessor.createCapture(retryablePaymentStateContext.isApiPayment(),retryablePaymentStateContext.getAttemptId(),retryablePaymentStateContext.getAccount(),retryablePaymentStateContext.getPaymentId(),retryablePaymentStateContext.getAmount(),retryablePaymentStateContext.getCurrency(),retryablePaymentStateContext.getPaymentTransactionExternalKey(),false,retryablePaymentStateContext.getProperties(),retryablePaymentStateContext.getCallContext(),retryablePaymentStateContext.getInternalCallContext());
}
