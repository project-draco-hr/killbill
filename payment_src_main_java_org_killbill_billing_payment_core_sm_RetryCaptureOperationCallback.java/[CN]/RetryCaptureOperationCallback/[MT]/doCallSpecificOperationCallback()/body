{
  return directPaymentProcessor.createCapture(retryableDirectPaymentStateContext.isApiPayment(),retryableDirectPaymentStateContext.getAccount(),retryableDirectPaymentStateContext.getPaymentMethodId(),retryableDirectPaymentStateContext.getAmount(),retryableDirectPaymentStateContext.getCurrency(),retryableDirectPaymentStateContext.getDirectPaymentTransactionExternalKey(),false,retryableDirectPaymentStateContext.getProperties(),retryableDirectPaymentStateContext.getCallContext(),retryableDirectPaymentStateContext.getInternalCallContext());
}
