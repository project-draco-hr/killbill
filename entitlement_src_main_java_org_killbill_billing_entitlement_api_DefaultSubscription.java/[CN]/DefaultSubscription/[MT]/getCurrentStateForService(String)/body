{
  if (currentSubscriptionBlockingStatesForServices == null) {
    return null;
  }
 else {
    final BlockingState blockingState=Iterables.<BlockingState>tryFind(currentSubscriptionBlockingStatesForServices,new Predicate<BlockingState>(){
      @Override public boolean apply(      final BlockingState input){
        return serviceName.equals(input.getService());
      }
    }
).orNull();
    return blockingState == null ? null : blockingState.getService();
  }
}
