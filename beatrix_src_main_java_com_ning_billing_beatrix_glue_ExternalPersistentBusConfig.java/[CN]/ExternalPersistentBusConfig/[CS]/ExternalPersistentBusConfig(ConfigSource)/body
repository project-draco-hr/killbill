{
  internalPersistentBusConfig=new ConfigurationObjectFactory(configSource).buildWithReplacements(PersistentBusConfig.class,ImmutableMap.<String,String>of("instanceName","main"));
  externalPersistentBusConfig=new ConfigurationObjectFactory(configSource).buildWithReplacements(PersistentBusConfig.class,ImmutableMap.<String,String>of("instanceName","external"));
}
