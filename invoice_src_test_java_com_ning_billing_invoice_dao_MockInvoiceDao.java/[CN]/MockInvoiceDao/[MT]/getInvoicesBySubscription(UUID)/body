{
  List<Invoice> result=new ArrayList<Invoice>();
synchronized (monitor) {
    for (    Invoice invoice : invoices.values()) {
      for (      InvoiceItem item : invoice.getInvoiceItems()) {
        if (subscriptionId.equals(item.getSubscriptionId())) {
          result.add(invoice);
          break;
        }
      }
    }
  }
  return result;
}
