{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  final SourceAndId that=(SourceAndId)o;
  if (sourceId != that.sourceId) {
    return false;
  }
  if (source != null ? !source.equals(that.source) : that.source != null) {
    return false;
  }
  return true;
}
