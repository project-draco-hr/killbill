{
  return paginationHelper.getPagination(TagSqlDao.class,new PaginationIteratorBuilder<TagModelDao,Tag,TagSqlDao>(){
    @Override public Iterator<TagModelDao> build(    final TagSqlDao tagSqlDao,    final Long limit){
      return tagSqlDao.searchTags(searchKey,String.format("%%%s%%",searchKey),offset,limit,context);
    }
  }
,offset,limit,context);
}
