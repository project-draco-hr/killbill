{
  Product P1=createProduct("FP");
  Product P2=createProduct("BP");
  Catalog c=createCatalog(new PlanPolicyChangeRule[]{new PlanPolicyChangeRule(Qualifier.TERM_FROM_LONG_TO_SHORT,END_OF_TERM,null)},null,null,P1,P2);
  assertEquals(END_OF_TERM,c.getPlanChangePolicy(new PlanPhaseSpecifier("FP",null,ANNUAL,null,EVERGREEN),new PlanSpecifier("BP",null,MONTHLY,null)));
  assertNull(c.getPlanChangePolicy(new PlanPhaseSpecifier("FP",null,MONTHLY,null,EVERGREEN),new PlanSpecifier("BP",null,ANNUAL,null)));
}
