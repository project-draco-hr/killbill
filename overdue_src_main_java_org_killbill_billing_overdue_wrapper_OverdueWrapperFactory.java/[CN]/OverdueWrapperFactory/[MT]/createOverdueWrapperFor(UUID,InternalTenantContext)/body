{
  try {
    final ImmutableAccountData account=accountApi.getImmutableAccountDataById(id,context);
    return new OverdueWrapper(account,api,getOverdueStateSet(context),clock,billingStateCalculator,overdueStateApplicator);
  }
 catch (  AccountApiException e) {
    throw new OverdueException(e);
  }
}
