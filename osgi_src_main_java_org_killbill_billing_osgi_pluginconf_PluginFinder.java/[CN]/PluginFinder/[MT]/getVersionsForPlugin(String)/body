{
  loadPluginsIfRequired();
  final List<T> result=new LinkedList<T>();
  for (  final String pluginName : allPlugins.keySet()) {
    if (pluginName.equals(lookupName)) {
      for (      final PluginConfig cur : allPlugins.get(pluginName)) {
        result.add((T)cur);
      }
    }
  }
  return result;
}
