{
  loadPluginsIfRequired();
  final List<T> result=new LinkedList<T>();
  for (  final String pluginName : allPlugins.keySet()) {
    final T plugin=(T)allPlugins.get(pluginName).get(0);
    if (pluginLanguage != plugin.getPluginLanguage()) {
      continue;
    }
    result.add(plugin);
  }
  return result;
}
