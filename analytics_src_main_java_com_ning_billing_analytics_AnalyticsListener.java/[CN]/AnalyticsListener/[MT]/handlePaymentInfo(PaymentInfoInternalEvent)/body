{
  updateWithAccountLock(paymentInfo.getAccountRecordId(),new Callable<Void>(){
    @Override public Void call() throws Exception {
      bipDao.invoicePaymentPosted(paymentInfo.getAccountId(),paymentInfo.getPaymentId(),paymentInfo.getStatus().toString(),createCallContext(paymentInfo));
      return null;
    }
  }
);
}
