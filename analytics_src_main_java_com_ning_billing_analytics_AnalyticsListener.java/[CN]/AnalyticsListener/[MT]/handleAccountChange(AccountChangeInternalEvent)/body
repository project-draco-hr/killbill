{
  if (!event.hasChanges()) {
    return;
  }
  GlobalLock lock=null;
  try {
    lock=locker.lockWithNumberOfTries(LockerType.ACCOUNT_FOR_ANALYTICS,event.getAccountId().toString(),NB_LOCK_TRY);
    bacDao.accountUpdated(event.getAccountId(),createCallContext(event));
  }
  finally {
    if (lock != null) {
      lock.release();
    }
  }
}
