{
  updateWithAccountLock(paymentError.getAccountRecordId(),new Callable<Void>(){
    @Override public Void call() throws Exception {
      bipDao.invoicePaymentPosted(paymentError.getAccountId(),paymentError.getPaymentId(),null,null,paymentError.getMessage(),createCallContext(paymentError));
      return null;
    }
  }
);
}
