{
  final String url=request.getRequestURI();
  for (int i=0; i < url.lastIndexOf('/'); i++) {
    final int idx=url.indexOf('/',i);
    if (idx > -1) {
      final String resourceName=url.substring(idx);
      final URL match=findResourceURL(resourceName);
      if (match != null) {
        return match;
      }
    }
  }
  return null;
}
