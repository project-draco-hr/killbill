{
  int lastIndexPriorFinalBracket=input.lastIndexOf("}");
  final StringBuilder tmp=new StringBuilder(input.substring(0,lastIndexPriorFinalBracket));
  tmp.append(",\"accountRecordId\":");
  tmp.append(accountRecordId);
  tmp.append(",\"tenantRecordId\":");
  tmp.append(tenantRecordId);
  tmp.append("}");
  return tmp.toString();
}
