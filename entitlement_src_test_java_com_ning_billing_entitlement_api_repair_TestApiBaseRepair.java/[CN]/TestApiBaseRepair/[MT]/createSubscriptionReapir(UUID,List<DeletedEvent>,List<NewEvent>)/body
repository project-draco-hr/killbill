{
  return new SubscriptionRepair(){
    @Override public UUID getId(){
      return id;
    }
    @Override public List<NewEvent> getNewEvents(){
      return newEvents;
    }
    @Override public List<ExistingEvent> getExistingEvents(){
      return null;
    }
    @Override public List<DeletedEvent> getDeletedEvents(){
      return deletedEvents;
    }
  }
;
}
