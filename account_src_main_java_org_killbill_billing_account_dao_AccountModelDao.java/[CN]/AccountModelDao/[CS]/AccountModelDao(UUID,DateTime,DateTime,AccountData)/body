{
  this(id,createdDate,updatedDate,account.getExternalKey(),account.getEmail(),account.getName(),account.getFirstNameLength(),account.getCurrency(),MoreObjects.firstNonNull(account.getBillCycleDayLocal(),DEFAULT_BILLING_CYCLE_DAY_LOCAL),account.getPaymentMethodId(),account.getTimeZone(),account.getLocale(),account.getAddress1(),account.getAddress2(),account.getCompanyName(),account.getCity(),account.getStateOrProvince(),account.getCountry(),account.getPostalCode(),account.getPhone(),account.isMigrated(),MoreObjects.firstNonNull(account.isNotifiedForInvoices(),false));
}
