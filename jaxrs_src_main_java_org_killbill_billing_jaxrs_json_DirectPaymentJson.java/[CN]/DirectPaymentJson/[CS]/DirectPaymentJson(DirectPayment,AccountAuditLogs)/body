{
  this(dp.getAccountId().toString(),dp.getId().toString(),dp.getPaymentNumber().toString(),dp.getAuthAmount(),dp.getCapturedAmount(),dp.getRefundedAmount(),dp.getCurrency() != null ? dp.getCurrency().toString() : null,dp.getPaymentMethodId() != null ? dp.getPaymentMethodId().toString() : null,getTransactions(dp.getTransactions(),dp.getId(),dp.getExternalKey(),accountAuditLogs),toAuditLogJson(accountAuditLogs == null ? null : accountAuditLogs.getAuditLogsForDirectPayment(dp.getId())));
}
