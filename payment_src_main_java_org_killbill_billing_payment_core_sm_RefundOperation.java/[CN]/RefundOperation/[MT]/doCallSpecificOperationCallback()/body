{
  logger.debug("Starting REFUND for payment {} ({} {})",paymentStateContext.getPaymentId(),paymentStateContext.getAmount(),paymentStateContext.getCurrency());
  return plugin.refundPayment(paymentStateContext.getAccount().getId(),paymentStateContext.getPaymentId(),paymentStateContext.getTransactionPaymentId(),paymentStateContext.getPaymentMethodId(),paymentStateContext.getAmount(),paymentStateContext.getCurrency(),paymentStateContext.getProperties(),paymentStateContext.getCallContext());
}
