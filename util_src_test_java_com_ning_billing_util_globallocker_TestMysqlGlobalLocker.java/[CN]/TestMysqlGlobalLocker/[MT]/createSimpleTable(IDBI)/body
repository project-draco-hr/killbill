{
  dbi.inTransaction(new TransactionCallback<Void>(){
    @Override public Void inTransaction(    Handle h,    TransactionStatus status) throws Exception {
      h.execute("DROP TABLE IF EXISTS dummy;");
      h.execute("create table dummy " + "(id int(11) unsigned NOT NULL AUTO_INCREMENT, " + "dummy_id char(36) NOT NULL, "+ "value varchar(256) NOT NULL,"+ "PRIMARY KEY(id)"+ ") ENGINE=innodb;");
      return null;
    }
  }
);
}
