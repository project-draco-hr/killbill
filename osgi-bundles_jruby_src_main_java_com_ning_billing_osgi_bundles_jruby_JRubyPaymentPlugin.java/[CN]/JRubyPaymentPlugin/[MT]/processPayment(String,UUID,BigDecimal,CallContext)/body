{
  checkValidPaymentPlugin();
  checkPluginIsRunning();
  final Ruby runtime=getRuntime();
  pluginInstance.callMethod("charge",JavaEmbedUtils.javaToRuby(runtime,externalAccountKey),JavaEmbedUtils.javaToRuby(runtime,paymentId.toString()),JavaEmbedUtils.javaToRuby(runtime,amount.longValue() * 100));
  return null;
}
