{
  checkValidPaymentPlugin();
  checkPluginIsRunning();
  final Ruby runtime=getRuntime();
  pluginInstance.callMethod("refund",JavaEmbedUtils.javaToRuby(runtime,account.getExternalKey()),JavaEmbedUtils.javaToRuby(runtime,paymentId.toString()),JavaEmbedUtils.javaToRuby(runtime,refundAmount.longValue() * 100));
}
