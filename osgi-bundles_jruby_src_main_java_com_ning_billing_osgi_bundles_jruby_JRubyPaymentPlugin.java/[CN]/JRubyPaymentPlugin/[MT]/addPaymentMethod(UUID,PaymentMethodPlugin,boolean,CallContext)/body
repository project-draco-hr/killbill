{
  checkValidPaymentPlugin();
  checkPluginIsRunning();
  final Ruby runtime=getRuntime();
  pluginInstance.callMethod("add_payment_method",JavaEmbedUtils.javaToRuby(runtime,kbPaymentMethodId.toString()),JavaEmbedUtils.javaToRuby(runtime,paymentMethodProps));
  if (setDefault) {
    setDefaultPaymentMethod(kbPaymentMethodId,context);
  }
}
