{
  testListener.pushExpectedEvent(NextEvent.CREATE);
  SubscriptionData subscription=(SubscriptionData)entitlementApi.createSubscription(bundle.getId(),new PlanPhaseSpecifier(productName,ProductCategory.BASE,term,planSet,null),clock.getUTCNow());
  assertNotNull(subscription);
  assertTrue(testListener.isCompleted(5000));
  return subscription;
}
