{
  return new ProductSpecifier(){
    @Override public BillingPeriod getBillingPeriod(){
      return term;
    }
    @Override public PhaseType getInitialPhaseType(){
      return phaseType;
    }
    @Override public String getPriceList(){
      return priceList;
    }
    @Override public String getProductName(){
      return productName;
    }
  }
;
}
