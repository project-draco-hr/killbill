{
  final String name="First" + RandomStringUtils.random(5) + " "+ "Last"+ RandomStringUtils.random(5);
  final Account account=new AccountBuilder(UUID.randomUUID()).name(name).firstNameLength(name.length()).externalKey("12345").phone("123-456-7890").email("user@example.com").currency(Currency.USD).billingCycleDay(1).build();
  accountDao.create(account);
  return account;
}
