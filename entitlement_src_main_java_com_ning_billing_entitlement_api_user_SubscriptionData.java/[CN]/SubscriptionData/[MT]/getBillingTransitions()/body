{
  if (transitions == null) {
    return Collections.emptyList();
  }
  List<SubscriptionEventTransition> result=new ArrayList<SubscriptionEventTransition>();
  SubscriptionTransitionDataIterator it=new SubscriptionTransitionDataIterator(clock,transitions,Order.ASC_FROM_PAST,Kind.BILLING,Visibility.ALL,TimeLimit.ALL);
  while (it.hasNext()) {
    result.add(it.next());
  }
  return result;
}
