{
  if (transitions == null) {
    return Collections.emptyList();
  }
  final List<EffectiveSubscriptionInternalEvent> result=new ArrayList<EffectiveSubscriptionInternalEvent>();
  final SubscriptionTransitionDataIterator it=new SubscriptionTransitionDataIterator(clock,transitions,Order.ASC_FROM_PAST,Kind.BILLING,Visibility.ALL,TimeLimit.ALL);
  while (it.hasNext()) {
    result.add(new DefaultEffectiveSubscriptionEvent(it.next(),alignStartDate));
  }
  return result;
}
