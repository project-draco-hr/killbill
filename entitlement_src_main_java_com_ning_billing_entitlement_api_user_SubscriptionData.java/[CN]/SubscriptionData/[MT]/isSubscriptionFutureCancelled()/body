{
  if (transitions == null) {
    return false;
  }
  SubscriptionTransitionDataIterator it=new SubscriptionTransitionDataIterator(clock,transitions,Order.ASC_FROM_PAST,Kind.ENTITLEMENT,Visibility.ALL,TimeLimit.FUTURE_ONLY);
  while (it.hasNext()) {
    SubscriptionTransitionData cur=it.next();
    if (cur.getTransitionType() == SubscriptionTransitionType.CANCEL) {
      return true;
    }
  }
  return false;
}
