{
  if (transitions == null) {
    return null;
  }
  for (  SubscriptionTransition cur : transitions) {
    if (cur.getTransitionType() == SubscriptionTransitionType.MIGRATE_BILLING) {
      continue;
    }
    if (cur.getEffectiveTransitionTime().isAfter(clock.getUTCNow())) {
      return cur;
    }
  }
  return null;
}
