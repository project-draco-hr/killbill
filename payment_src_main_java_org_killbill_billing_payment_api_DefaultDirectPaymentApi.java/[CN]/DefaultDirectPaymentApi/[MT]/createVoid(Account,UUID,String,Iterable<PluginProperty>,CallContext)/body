{
  checkNotNullArgumentList(account,directPaymentId,directPaymentTransactionExternalKey,properties,callContext);
  final InternalCallContext internalCallContext=internalCallContextFactory.createInternalCallContext(account.getId(),callContext);
  return directPaymentProcessor.createVoid(account,directPaymentId,directPaymentTransactionExternalKey,SHOULD_LOCK_ACCOUNT,properties,callContext,internalCallContext);
}
