{
  for (  DefaultLimit limit : limits) {
    limit.initialize(root,uri);
    limit.setUsage(this);
  }
  for (  DefaultBlock block : blocks) {
    block.initialize(root,uri);
    block.setUsage(this);
  }
  for (  DefaultTier tier : tiers) {
    tier.initialize(root,uri);
    tier.setUsage(this);
  }
}
