{
  return super.evaluate((BillingState)state,now) && (basePlanProduct == null || basePlanProduct.equals(state.getBasePlanProduct())) && (basePlanBillingPeriod == null || basePlanBillingPeriod.equals(state.getBasePlanBillingPeriod()))&& (basePlanPriceList == null || basePlanPriceList.equals(state.getBasePlanPriceList()))&& (basePlanPhaseType == null || basePlanPhaseType.equals(state.getBasePlanPhaseType()));
}
