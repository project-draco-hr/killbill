{
  this(input.getBundleSymbolicName(),input.getPluginName(),input.getVersion(),input.isRunning(),ImmutableSet.copyOf(Iterables.transform(input.getServices(),new Function<PluginServiceInfo,PluginServiceInfoJson>(){
    @Override public PluginServiceInfoJson apply(    final PluginServiceInfo input){
      return new PluginServiceInfoJson(input.getServiceTypeName(),input.getRegistrationName());
    }
  }
)));
}
